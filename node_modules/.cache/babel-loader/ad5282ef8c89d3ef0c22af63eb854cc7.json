A,CAAA,UAAA,KAAA,EAAA;AACA,sBAAA,KAAA,EAAA;AACA,oBAAA,KAAA,CAAA,KAAA,CAAA;AAAA,sBAAA,QAAA,EAAA,KAAA,CAAA,KAAA;AAAA,sBAAA,QAAA,EAAA,KAAA,CAAA;AAAA,qBAAA;AACA,mBAFA,MAEA;AACA,oBAAA,KAAA,CAAA,MAAA,CAAA,KAAA,CAAA,EAAA,CAAA,+BAAA,CAAA,EAAA;AAAA,sBAAA,IAAA,EAAA;AAAA,qBAAA;AACA;AACA,iBANA;;AADA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AASA,KAbA;AAcA,IAAA,UAdA,sBAcA,IAdA,EAcA,GAdA,EAcA,KAdA,EAcA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AACA,gBAAA,QADA,GACA;AACA,kBAAA,UAAA,EAAA,IADA;AAEA,kBAAA,GAAA,EAAA,GAFA;AAGA,kBAAA,IAAA,EAAA,KAHA;AAIA,kBAAA,QAAA,EAAA,EAJA;AAKA,kBAAA,QAAA,EAAA;AALA,iBADA;;AAQA,gBAAA,MAAA,CAAA,YAAA,CAAA,QAAA;;AARA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AASA;AAvBA,IAzBA;AAkDA,EAAA,OAlDA,qBAkDA;AACA,IAAA,QAAA,CAAA,IAAA,CAAA,SAAA,CAAA,GAAA,CAAA,cAAA;AACA;AApDA,CAAA","sourcesContent":["<script>\r\nimport { mapActions } from 'vuex'\r\nimport appConfig from \"@/app.config\";\r\nimport AuthLayout from \"@/shared/layouts/auth\"\r\nimport ESigns from './components/ESigns'\r\n/**\r\n * Login component\r\n */\r\nexport default {\r\n    components: {\r\n        AuthLayout,\r\n        ESigns\r\n    },\r\n    page: {\r\n        title: \"Login\",\r\n        meta: [\r\n            {\r\n                name: \"description\",\r\n                content: appConfig.description,\r\n            },\r\n        ],\r\n    },\r\n    data () {\r\n        return {\r\n            email: \"\",\r\n            password: \"\",\r\n            submitted: false,\r\n            showPassword: false,\r\n            authError: null,\r\n            tryingToLogIn: false,\r\n            isAuthError: false,\r\n        };\r\n    },\r\n    methods: {\r\n        ...mapActions(\"auth\", [\"login\", \"loginByToken\"]),\r\n        // Try to log the user in with the username\r\n        // and password they provided.\r\n        async tryToLogIn () {\r\n            this.$refs.observer.validate().then(valid => {\r\n                if (valid) {\r\n                    this.login({ Username: this.email, Password: this.password })\r\n                } else {\r\n                    this.$toast(this.$t('messages.fill_required_fields'), { type: 'error' });\r\n                }\r\n            })\r\n\r\n        },\r\n        async loginByEsp (data, tin, pinfl) {\r\n            let authData = {\r\n                signedData: data,\r\n                TIN: tin,\r\n                PNFL: pinfl,\r\n                username: \"\",\r\n                password: \"\",\r\n            };\r\n            this.loginByToken(authData)\r\n        },\r\n    },\r\n    mounted () {\r\n        document.body.classList.add(\"auth-body-bg\");\r\n    },\r\n};\r\n</script>\r\n\r\n<template>\r\n    <AuthLayout>\r\n        <ESigns\r\n            v-if=\"$route.query.login_page == 'ERI_KEY'\"\r\n            for-login\r\n            @sign=\"loginByEsp\"\r\n            @back=\"$router.push({name: 'Login'})\"\r\n        ></ESigns>\r\n\r\n        <div\r\n            v-else\r\n            class=\"mt-4 auth-login-wrapper\"\r\n        >\r\n            <ValidationObserver\r\n                ref=\"observer\"\r\n                v-slot=\"{}\"\r\n            >\r\n                <b-row class=\"mb-3\">\r\n                    <b-col\r\n                        cols=\"12\"\r\n                        class=\"mb-3\"\r\n                    >\r\n                        <BaseInputWithValidation\r\n                            name=\"username\"\r\n                            rules=\"required\"\r\n                            ref=\"username\"\r\n                            label-on-top\r\n                            v-model=\"email\"\r\n                            :label=\"$t('auth.username')\"\r\n                            class=\"required\"\r\n                        />\r\n                    </b-col>\r\n                    <b-col\r\n                        cols=\"12\"\r\n                        class=\"mb-1\"\r\n                    >\r\n                        <BaseInputWithValidation\r\n                            name=\"password\"\r\n                            with-input-append\r\n                            label-on-top\r\n                            class=\"required\"\r\n                            @keyup.enter=\"tryToLogIn\"\r\n                            :rules=\"{'required':true}\"\r\n                            :type=\"showPassword ? 'text' : 'password'\"\r\n                            v-model=\"password\"\r\n                            :label=\"$t('auth.password')\"\r\n                        >\r\n                            <template v-slot:append-slot>\r\n                                <b-button\r\n                                    @click=\"showPassword = !showPassword\"\r\n                                    variant=\"outline-primary\"\r\n                                    id=\"contractorSearchButton\"\r\n                                    style=\"padding: 2.5px 10px; font-size: 1.2rem;\"\r\n                                >\r\n                                    <i :class=\"showPassword ? 'mdi mdi-eye-off' : 'mdi mdi-eye'\"></i>\r\n                                </b-button>\r\n                            </template>\r\n                        </BaseInputWithValidation>\r\n                    </b-col>\r\n                </b-row>\r\n            </ValidationObserver>\r\n            <div class=\"mt-3 d-grid\">\r\n                <b-button\r\n                    @click=\"tryToLogIn\"\r\n                    variant=\"primary\"\r\n                >{{ $t('auth.login') }}</b-button>\r\n            </div>\r\n\r\n            <div class=\"mt-3 d-grid\">\r\n                <b-button\r\n                    class=\"btn-block\"\r\n                    to=\"/login?login_page=ERI_KEY\"\r\n                    variant=\"outline-primary\"\r\n                >{{ $t('auth.login_with_e_signature') }}</b-button>\r\n            </div>\r\n\r\n            <!-- <div class=\"mt-4 text-center\">\r\n                <h5 class=\"font-size-14 mb-3\">{{ $t('auth.login_with_e_signature') }}</h5>\r\n            </div> -->\r\n            <div class=\"mt-5 text-center\">\r\n                <p>\r\n                    {{ $t('auth.not_yet_registered') }}\r\n                    <router-link\r\n                        :to=\"{name: 'Register'}\"\r\n                        class=\"fw-medium text-primary\"\r\n                    >\r\n                        {{ $t('auth.registration') }}\r\n                    </router-link>\r\n                </p>\r\n            </div>\r\n            <div class=\"mt-4 text-center\">\r\n                <router-link\r\n                    tag=\"a\"\r\n                    to=\"#\"\r\n                    class=\"text-muted\"\r\n                >\r\n                    <i class=\"mdi mdi-lock me-1\"></i> {{ $t('auth.forgot_password') }}\r\n                </router-link>\r\n            </div>\r\n        </div>\r\n    </AuthLayout>\r\n</template>\r\n\r\n<style lang=\"scss\" scoped>\r\n.auth-login-wrapper {\r\n    width: 40%;\r\n    margin-right: auto;\r\n    margin-left: auto;\r\n    @media (max-width: 768px) {\r\n        width: 90%;\r\n    }\r\n}\r\n</style>"],"sourceRoot":"src/shared/views/auth"}]}