"\r\n>\r\n  <i class=\"fa fa-arrow-left\"></i>\r\n</b-button> -->\r\n            <h5\r\n                class=\"float-right m-0\"\r\n                v-if=\"numPages\"\r\n            >\r\n              {{ currentPage }} / {{ numPages }}\r\n            </h5>\r\n          </div>\r\n          <div style=\"width: 43%\">\r\n            <div class=\"float-right d-flex align-items-center\">\r\n              <b-button-group>\r\n                <b-button\r\n                    class=\"float-right\"\r\n                    @click=\"signData\"\r\n                    variant=\"primary\"\r\n                >\r\n                  <b-overlay\r\n                      :opacity=\"0.1\"\r\n                      :show=\"loaderQrCode\"\r\n                      rounded=\"sm\"\r\n                  >\r\n                    <i class=\"fa fa-qrcode mr-1\"></i>\r\n                    {{ $t(\"actions.qrcode\") }}\r\n                  </b-overlay>\r\n                </b-button>\r\n                <b-button\r\n                    class=\"float-right mr-2\"\r\n                    @click=\"save\"\r\n                    variant=\"success\"\r\n                >\r\n                  <i class=\"fa fa-save\"></i>\r\n                  {{ $t(\"actions.save\") }}\r\n                </b-button>\r\n              </b-button-group>\r\n\r\n              <b-button\r\n                  class=\"float-right mr-2\"\r\n                  :to=\"{name: 'LetterCreate'}\"\r\n                  variant=\"primary\"\r\n              >\r\n                <i class=\"fa fa-arrow-left\"></i>\r\n                <!-- {{ $t(\"actions.back\") }} -->\r\n              </b-button>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n      <div class=\"mt-5\">\r\n        <b-row>\r\n          <!-- <b-col></b-col> -->\r\n          <b-col cols=\"12\">\r\n            <div class=\"d-flex justify-content-center\">\r\n              <b-overlay\r\n                  variant=\"white\"\r\n                  :opacity=\"1\"\r\n