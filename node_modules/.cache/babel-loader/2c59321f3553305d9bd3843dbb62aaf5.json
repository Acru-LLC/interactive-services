\n                                    md=\"2\"\r\n                                    class=\"pe-0\"\r\n                                >\r\n                                    <b-skeleton-img\r\n                                        no-aspect\r\n                                        height=\"10rem\"\r\n                                    ></b-skeleton-img>\r\n                                </b-col>\r\n                                <b-col\r\n                                    cols=\"3\"\r\n                                    md=\"2\"\r\n                                    class=\"pe-0\"\r\n                                >\r\n                                    <b-skeleton-img\r\n                                        no-aspect\r\n                                        height=\"10rem\"\r\n                                    ></b-skeleton-img>\r\n                                </b-col>\r\n                                <b-col\r\n                                    cols=\"4\"\r\n                                    class=\"mt-2 mt-md-0\"\r\n                                >\r\n                                    <b-skeleton width=\"85%\"></b-skeleton>\r\n                                    <b-skeleton width=\"55%\"></b-skeleton>\r\n                                    <b-skeleton width=\"70%\"></b-skeleton>\r\n                                    <b-skeleton width=\"70%\"></b-skeleton>\r\n                                </b-col>\r\n                            </b-row>\r\n                        </div>\r\n                    </template>\r\n                    <template v-else>\r\n                        <div class=\"ad-details-wrapper\">\r\n                            <table class=\"ad-details\">\r\n                                <tbody>\r\n                                    <tr>\r\n                                        <th scope=\"row\">{{ $t('column.ad_construction_owner')}}</th>\r\n                                        <td>{{ currentAdDetails.contract