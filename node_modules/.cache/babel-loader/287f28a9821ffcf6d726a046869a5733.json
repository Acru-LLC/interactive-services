    page: 0,\r\n        itemsPerPage: 20,\r\n        search: v\r\n      });\r\n    },\r\n  },\r\n  components: {\r\n    Contacts,\r\n  },\r\n  validations: {\r\n    name: {\r\n      // required,\r\n    },\r\n  },\r\n  data() {\r\n    return {\r\n      name: \"\",\r\n      isMembers: false,\r\n      searchValue: \"\",\r\n      members: [],\r\n    };\r\n  },\r\n  methods: {\r\n    pushMember(v) {\r\n      let index = this.members.indexOf(v.employeeId);\r\n      if (index > -1) {\r\n        this.members.splice(index, 1);\r\n      } else {\r\n        this.members.unshift(v.employeeId);\r\n      }\r\n    },\r\n    save() {\r\n      messengerService\r\n        .createGroupChat({\r\n          groupName: this.name,\r\n          employeeIds: this.members,\r\n        })\r\n        .then((rs) => {\r\n          this.members = [];\r\n          this.name = \"\";\r\n          this.searchValue = \"\";\r\n          this.isMembers = false;\r\n          this.$emit(\"cancel\", \"success\");\r\n        })\r\n        .catch((e) => {\r\n          // catchErr(e);\r\n        });\r\n    },\r\n    reset() {\r\n      this.name = \"\";\r\n    },\r\n    next() {\r\n      if (!this.name || this.name.trim().length == 0) {\r\n        return;\r\n      }\r\n      this.isMembers = true;\r\n      setTimeout(() => {\r\n        this.$refs.contactComponent.getClickTab({ page: 0, itemsPerPage: 20 });\r\n      }, 200);\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n</style>"],"sourceRoot":"src/modules/chat"}]}