;AACA,WAAA,eAAA,CAAA,KAAA,cAAA;AACA,KAlBA;AAmBA,IAAA,eAnBA,2BAmBA,QAnBA,EAmBA;AAAA;;AACA,WAAA,cAAA,GAAA,QAAA;;AACA,UAAA,KAAA,kBAAA,CAAA,MAAA,EAAA;AACA,aAAA,WAAA,CAAA,KAAA,cAAA,EAAA,OAAA,GAAA,KAAA,aAAA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AACA,QAAA,mBAAA,CACA,qBADA,CACA,YADA,EACA,KAAA,WAAA,CAAA,KAAA,cAAA,CADA,EACA,KAAA,kBAAA,CAAA,KAAA,cAAA,EAAA,IAAA,CAAA,WAAA,EADA,EACA,IADA,EAEA,IAFA,CAEA,UAAA,GAAA,EAAA;AACA,UAAA,MAAA,CAAA,UAAA,GAAA,GAAA,CAAA,IAAA,CAAA,IAAA,CADA,CAEA;;AACA,UAAA,MAAA,CAAA,WAAA,CAAA,MAAA,CAAA,cAAA,EAAA,UAAA,GAAA,GAAA,CAAA,IAAA,CAAA,KAAA;AACA,SANA,EAOA,KAPA,CAOA,UAAA,CAAA,EAAA;AACA,UAAA,MAAA,CAAA,UAAA,GAAA,EAAA,CADA,CAEA;;AACA,UAAA,MAAA,CAAA,WAAA,CAAA,MAAA,CAAA,cAAA,EAAA,UAAA,GAAA,GAAA,CAAA,IAAA,CAAA,KAAA;AACA,SAXA;AAYA;AACA,KA5CA;AA6CA,IAAA,QA7CA,oBA6CA,EA7CA,EA6CA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA;AAAA,QAAA,IAAA,EAAA,wBAAA;AAAA,QAAA,MAAA,EAAA;AAAA,UAAA,EAAA,EAAA,EAAA;AAAA,UAAA,SAAA,EAAA,KAAA,kBAAA,CAAA,KAAA,cAAA,EAAA,IAAA,CAAA,WAAA;AAAA;AAAA,OAAA;AACA,KA/CA;AAgDA,IAAA,UAhDA,sBAgDA,EAhDA,EAgDA;AAAA;;AACA,WAAA,QAAA,CAAA,aAAA,CAAA,KAAA,EAAA,CAAA,uBAAA,CAAA,EAAA;AACA,QAAA,OAAA,EAAA,KAAA,EAAA,CAAA,iBAAA,CADA;AAEA,QAAA,WAAA,EAAA,KAAA,EAAA,CAAA,gBAAA;AAFA,OAAA,EAIA,IAJA,CAIA,UAAA,KAAA,EAAA;AACA,YAAA,KAAA,EAAA;AACA,UAAA,mBAAA,CACA,UADA,CACA,YADA,EACA,EADA,EAEA,IAFA,CAEA,UAAA,GAAA,EAAA;AACA,YAAA,MAAA,CAAA,eAAA,CAAA,MAAA,CAAA,cAAA;AACA,WAJA,EAKA,KALA,CAKA,UAAA,CAAA,EAAA;AACA,YAAA,OAAA,CAAA,GAAA,CAAA,CAAA;AACA,WAPA;AAQA;AACA,OAfA,EAgBA,KAhBA,CAgBA,UAAA,GAAA,EAAA,CACA;AACA,OAlBA;AAmBA;AApEA,GAnEA;;AAyIA;AACA,EAAA,OA1IA,qBA0IA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,qBAEA,aAAA,CAAA,YAAA,CAAA,mBAAA,EAAA,IAAA,EACA,IADA,CACA,UAAA,GAAA,EAAA;AACA,gBAAA,MAAA,CAAA,kBAAA,GAAA,GAAA,CAAA,IAAA,CAAA,QAAA;;AACA,gBAAA,MAAA,CAAA,kBAAA,CAAA,OAAA,CAAA,UAAA,EAAA,EAAA;AACA,sBAAA,cAAA,GAAA,MAAA,CAAA,MAAA,CAAA,EAAA,EAAA,MAAA,CAAA,0BAAA,CAAA;AACA,kBAAA,cAAA,CAAA,UAAA,GAAA,CAAA;;AACA,kBAAA,MAAA,CAAA,WAAA,CAAA,IAAA,CAAA,cAAA;AACA,iBAJA;AAKA;AACA;AACA;AACA;;AACA,eAZA,EAaA,KAbA,CAaA,UAAA,CAAA,EAAA;AACA,gBAAA,OAAA,CAAA,GAAA,CAAA,CAAA;AACA,eAfA,CAFA;;AAAA;AAAA;AAAA,qBAmBA,MAAA,CAAA,eAAA,CAAA,MAAA,CAAA,cAAA,CAnBA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAoBA,GA9JA;;AA+JA;;AACA;AACA;AACA;AACA;AACA;AACA;;AACA;AACA;AACA,EAAA,KAAA,EAAA;AACA;AACA;AACA;AACA;;AACA;AACA;AACA;AACA;AACA;AACA;AAVA;AAxKA,CAAA","sourcesContent":["<template>\r\n    <div class=\"row\">\r\n        <div class=\"col-12\">\r\n            <div class=\"card\">\r\n              <div class=\"ml-5 mt-3 mb-0\" style=\"text-align: center\">\r\n                <h4> {{ $t('submodules.product_or_services_daminiriushiy.title') }}</h4>\r\n              </div>\r\n                <div class=\"card-body\">\r\n                    <div class=\"row mb-2\">\r\n                        <div class=\"col-sm-8\">\r\n                            <div class=\"search-box me-4 mb-2 d-inline-block\">\r\n                                <div class=\"position-relative\">\r\n                                    <input\r\n                                        v-model=\"searchKeyword\"\r\n                                        type=\"text\"\r\n                                        class=\"form-control\"\r\n                                        @input=\"fetchTableItems(activeTabIndex)\"\r\n                                        :placeholder=\"$t('column.search')\"\r\n                                    />\r\n                                    <i class=\"bx bx-search-alt search-icon\"></i>\r\n                                </div>\r\n                            </div>\r\n                            <!-- <span>{{ $t('column.select.text1') }}</span> -->\r\n                            <div class=\"col-2 me-2 mx-2 d-inline-block\">\r\n                                <div class=\"position-relative\">\r\n                                    <b-form-select\r\n                                        v-model=\"selected\"\r\n                                        :options=\"optionsTable\"\r\n                                        @change=\"selectList\"\r\n                                        class=\"form-select\"\r\n                                    ></b-form-select>\r\n                                </div>\r\n                            </div>\r\n                            <!-- <span>{{ $t('column.select.text2') }}</span> -->\r\n                        </div>\r\n                        <div class=\"col-sm-4\">\r\n                            <div class=\"text-sm-end\">\r\n                                <b-row>\r\n                                    <b-col cols=\"7\">\r\n                                    </b-col>\r\n                                    <b-col cols=\"5\">\r\n                                        <b-btn\r\n                                            v-if=\"activeTabIndex > -1 && contractorStatuses.length\"\r\n                                            type=\"button\"\r\n                                            class=\"btn btn-success btn-rounded mb-2 me-2\"\r\n                                            :to=\"{name: 'CreateProductOrServiceDaminiriushiy', params: {cStatusId: contractorStatuses[activeTabIndex].code.toLowerCase()}}\"\r\n                                        >\r\n                                            <i class=\"mdi mdi-plus me-1\"></i> {{ $t('actions.add') }}\r\n                                        </b-btn>\r\n                                    </b-col>\r\n                                </b-row>\r\n                            </div>\r\n                        </div>\r\n                        <!-- end col-->\r\n                    </div>\r\n<!--                    <b-tabs-->\r\n<!--                        justified-->\r\n<!--                        nav-class=\"nav-tabs-custom\"-->\r\n<!--                        content-class=\"py-2\"-->\r\n<!--                        v-model=\"activeTabIndex\"-->\r\n<!--                    >-->\r\n<!--                        <b-tab-->\r\n<!--                            v-for=\"(cStatus, index) in contractorStatuses\"-->\r\n<!--                            :key=\"`product-or-service-types-by-contractor-status-${index}`\"-->\r\n<!--                            @click=\"fetchTableItems(index)\"-->\r\n<!--                        >-->\r\n<!--                            <template v-slot:title>-->\r\n<!--                                <span class=\"d-inline-block d-sm-none\">-->\r\n<!--                                    <i class=\"fas fa-home\"></i>-->\r\n<!--                                </span>-->\r\n<!--                                <span class=\"d-none d-sm-inline-block\">{{ getName({nameRu: cStatus.nameRu, nameUz: cStatus.nameUz, nameLt: cStatus.nameLt}) }}</span>-->\r\n<!--                            </template>-->\r\n                            <!-- Table data -->\r\n                            <b-table\r\n                                :items=\"tableItems\"\r\n                                :fields=\"tableFields\"\r\n                                class=\"custom-b-table\"\r\n                                responsive\r\n                                striped\r\n                                bordered\r\n                                small\r\n                                hover\r\n                                show-empty\r\n                            >\r\n                                <!-- NUMBER OF ITEM -->\r\n                                <template #cell(index)=\"data\">\r\n                                    {{ util_paginate(data.index, paginations[activeTabIndex].page, paginations[activeTabIndex].itemsPerPage) }}\r\n                                </template>\r\n\r\n                                <!-- NAME -->\r\n                                <template #cell(name)=\"data\">\r\n                                    <div class=\"d-flex justify-content-between\">\r\n                                        <p\r\n                                            class=\"mb-0 d-flex align-items-center\"\r\n                                            style=\"flex-basis: 0; flex-grow: 1; gap: .3rem;\"\r\n                                        ><span class=\"badge bg-primary\">ЎЗ</span> : <span> {{ data.item.nameUz }}</span></p>\r\n                                        <p\r\n                                            class=\"mb-0 d-flex align-items-center\"\r\n                                            style=\"flex-basis: 0; flex-grow: 1; gap: .3rem;\"\r\n                                        ><span class=\"badge bg-primary\">O'Z</span> : <span> {{ data.item.nameLt }}</span></p>\r\n                                        <p\r\n                                            class=\"mb-0 d-flex align-items-center\"\r\n                                            style=\"flex-basis: 0; flex-grow: 1; gap: .3rem;\"\r\n                                        ><span class=\"badge bg-primary\">РУ</span> : <span> {{ data.item.nameRu }}</span></p>\r\n                                    </div>\r\n                                </template>\r\n\r\n                                <!-- STATUS -->\r\n                                <template #cell(status)=\"data\">\r\n                                    <p>\r\n                                        {{\r\n                  getName({\r\n                    nameRu: data.item.statusNameRu,\r\n                    nameLt: data.item.statusNameLt,\r\n                    nameUz: data.item.statusNameUz,\r\n                  })\r\n                }}\r\n                                    </p>\r\n                                </template>\r\n\r\n                                <!-- PRODUCT_OR_SERVICE_TYPE -->\r\n                                <template #cell(product_or_service_type)=\"data\">\r\n                                    <p>\r\n                                        {{\r\n                  getName({\r\n                    nameRu: data.item.directoryProductOrServiceTypeNameRu,\r\n                    nameLt: data.item.directoryProductOrServiceTypeNameLt,\r\n                    nameUz: data.item.directoryProductOrServiceTypeNameUz,\r\n                  })\r\n                }}\r\n                                    </p>\r\n                                </template>\r\n\r\n                                <!-- ACTIONS -->\r\n                                <template #cell(actions)=\"data\">\r\n                                    <div class=\"general-table__actions d-flex justify-content-center\">\r\n                                        <b-btn\r\n                                            variant=\"link\"\r\n                                            class=\"text-decoration-none p-0\"\r\n                                            style=\"font-size: 1.2rem; margin-right: 1rem;\"\r\n                                        >\r\n                                            <i\r\n                                                @click=\"editItem(data.item.id)\"\r\n                                                class=\"mdi mdi-circle-edit-outline edit\"\r\n                                            ></i>\r\n                                        </b-btn>\r\n\r\n                                        <b-btn\r\n                                            variant=\"link\"\r\n                                            class=\"text-decoration-none p-0 text-danger\"\r\n                                            style=\"font-size: 1.2rem;\"\r\n                                        >\r\n                                            <i\r\n                                                @click=\"deleteItem(data.item.id)\"\r\n                                                class=\"mdi mdi-trash-can delete\"\r\n                                            ></i>\r\n                                        </b-btn>\r\n                                    </div>\r\n                                </template>\r\n\r\n                                <!-- EMPTY SLOT -->\r\n                                <template #empty=\"\">\r\n                                    <h4 class=\"text-center\">{{ $t('messages.data_not_found') }}</h4>\r\n                                </template>\r\n\r\n                                <!-- TABLE_BUSY SLOT -->\r\n                                <template #table-busy>\r\n                                    <div class=\"text-center my-2\">\r\n                                        <b-spinner\r\n                                            variant=\"primary\"\r\n                                            class=\"align-middle\"\r\n                                        ></b-spinner>\r\n\r\n                                    </div>\r\n                                </template>\r\n                            </b-table>\r\n\r\n                            <!-- end table -->\r\n                            <b-pagination\r\n                                :key=\"`tab-pagination-${activeTabIndex}`\"\r\n                                v-model=\"paginations[activeTabIndex].page\"\r\n                                @input=\"fetchTableItems(activeTabIndex)\"\r\n                                :total-rows=\"paginations[activeTabIndex].totalItems\"\r\n                                :per-page=\"paginations[activeTabIndex].itemsPerPage\"\r\n                                aria-controls=\"my-table\"\r\n                                class=\"justify-content-end\"\r\n                            ></b-pagination>\r\n<!--                        </b-tab>-->\r\n<!--                    </b-tabs>-->\r\n                </div>\r\n                <!-- end card-body -->\r\n            </div>\r\n            <!-- end card -->\r\n        </div>\r\n        <!-- end col -->\r\n    </div>\r\n    <!-- end row -->\r\n</template>\r\n\r\n<script>\r\nconst MAIN_API_URL = 'directory/product-or-services'\r\n// var cStatusIdFromRoute = null\r\nimport appConfig from \"@/app.config\";\r\nimport crudAndListsService from '@/shared/services/crud_and_list.service'\r\nimport helperService from '@/shared/services/helper.service'\r\n\r\nexport default {\r\n    page: {\r\n        title: \"Product Or Service Types\",\r\n        meta: [{ name: \"description\", content: appConfig.description }],\r\n    },\r\n    components: {},\r\n    data () {\r\n        return {\r\n            paginations: [],\r\n            activeTabIndex: 1,\r\n            contractorStatuses: [],\r\n            json_fields: {\r\n                \"Name Uz\": \"nameUz\",\r\n                \"Name Lt\": \"nameLt\",\r\n                \"Name Ru\": \"nameRu\",\r\n            },\r\n            json_data: [],\r\n            json_meta: [\r\n                [\r\n                    {\r\n                        key: \"charset\",\r\n                        value: \"utf-8\",\r\n                    },\r\n                ],\r\n            ],\r\n            searchKeyword: '',\r\n            selected: 20,\r\n            optionsTable: [\r\n                { value: 20, text: 20 },\r\n                { value: 50, text: 50 },\r\n                { value: 100, text: 100 },\r\n                { value: 150, text: 150 },\r\n                { value: 200, text: 200 },\r\n            ],\r\n            title: \"Product Or Service Type\",\r\n            activeDep: {},\r\n            tableItems: [],\r\n            totalItems: 0,\r\n            tableFields: [\r\n                {\r\n                    label: \"#\",\r\n                    thClass: \"text-center\",\r\n                    tdClass: \"text-center\",\r\n                    sortable: false,\r\n                    key: \"index\",\r\n                },\r\n                { label: this.$t('column.name'), key: \"name\" },\r\n                { label: this.$t('submodules.product_or_service_types.title'), key: \"product_or_service_type\" },\r\n                { label: this.$t('column.code'), key: \"code\" },\r\n                { label: this.$t('column.skp_code'), key: \"skpcode\" },\r\n                { label: this.$t('column.tnved_code'), key: \"tnvedcode\" },\r\n                { label: this.$t('column.status'), key: \"status\" },\r\n                {\r\n                    label: this.$t('column.actions'),\r\n                    key: \"actions\",\r\n                    thClass: \"text-center\",\r\n                    tdClass: \"text-center\",\r\n                    sortable: false\r\n                },\r\n            ],\r\n        };\r\n    },\r\n    /*\r\n    COMPUTED */\r\n    computed: {\r\n        \r\n    },\r\n    methods: {\r\n        downloadExcel () {\r\n            this.json_data = []\r\n            this.tableItems.forEach(res => {\r\n                let obj = {}\r\n                obj.nameUz = res.nameUz\r\n                obj.nameLt = res.nameLt\r\n                obj.nameRu = res.nameRu\r\n                this.json_data.push(obj)\r\n            })\r\n        },\r\n        selectList ($event) {\r\n            if ($event == 'all') {\r\n                this.paginations[this.activeTabIndex].itemsPerPage = this.totalItems\r\n            } else {\r\n                this.paginations[this.activeTabIndex].itemsPerPage = $event\r\n            }\r\n            this.fetchTableItems(this.activeTabIndex);\r\n        },\r\n        fetchTableItems (tabIndex) {\r\n            this.activeTabIndex = tabIndex\r\n            if (this.contractorStatuses.length) {\r\n                this.paginations[this.activeTabIndex].keyword = this.searchKeyword\r\n                /* this.var_default_search_payload.sortDesc.push(false)\r\n                if (this.$i18n.locale == 'uz') {\r\n                    this.var_default_search_payload.sortBy.push('nameLt')\r\n                } else if (this.$i18n.locale == 'ru') {\r\n                    this.var_default_search_payload.sortBy.push('nameRu')\r\n                } else if (this.$i18n.locale == 'uzCyrillic') {\r\n                    this.var_default_search_payload.sortBy.push('nameUz')\r\n                } */\r\n                crudAndListsService\r\n                    .searchListWithKeyword(MAIN_API_URL, this.paginations[this.activeTabIndex], this.contractorStatuses[this.activeTabIndex].code.toLowerCase(), true)\r\n                    .then((res) => {\r\n                        this.tableItems = res.data.list;\r\n                        // this.totalItems = res.data.total;\r\n                        this.paginations[this.activeTabIndex].totalItems = res.data.total\r\n                    })\r\n                    .catch(e => {\r\n                        this.tableItems = [];\r\n                        // this.totalItems = 0;\r\n                        this.paginations[this.activeTabIndex].totalItems = res.data.total\r\n                    })\r\n            }\r\n        },\r\n        editItem (id) {\r\n            this.$router.push({ name: 'UpdateProductOrService', params: { id: id, cStatusId: this.contractorStatuses[this.activeTabIndex].code.toLowerCase() } })\r\n        },\r\n        deleteItem (id) {\r\n            this.$bvModal.msgBoxConfirm(this.$t('messages.delete_title'), {\r\n                okTitle: this.$t('actions.confirm'),\r\n                cancelTitle: this.$t('actions.cancel')\r\n            })\r\n                .then(value => {\r\n                    if (value) {\r\n                        crudAndListsService\r\n                            .deleteById(MAIN_API_URL, id)\r\n                            .then((res) => {\r\n                                this.fetchTableItems(this.activeTabIndex)\r\n                            })\r\n                            .catch(e => {\r\n                                console.log(e)\r\n                            })\r\n                    }\r\n                })\r\n                .catch(err => {\r\n                    // An error occurred\r\n                })\r\n        },\r\n    },\r\n    /* CREATED */\r\n    async created () {\r\n        // GET CONTRACTOR_STATUSES\r\n        await helperService.getRefByCode('contractor_status', true)\r\n            .then(res => {\r\n                this.contractorStatuses = res.data.children\r\n                th