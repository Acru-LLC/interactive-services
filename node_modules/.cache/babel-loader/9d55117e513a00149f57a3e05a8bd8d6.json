,MAAA;AACA,QAAA,GAAA,CAAA,MAAA,GAAA,GAAA,CAAA,MAAA;AACA,QAAA,GAAA,CAAA,WAAA,CAAA,OAAA,CAAA,UAAA,CAAA,EAAA;AACA,UAAA,GAAA,CAAA,WAAA,CAAA,IAAA,CAAA,CAAA,CAAA,MAAA;AACA,SAFA;;AAGA,QAAA,KAAA,CAAA,SAAA,CAAA,IAAA,CAAA,GAAA;AACA,OAXA;AAYA,KAfA;AAgBA,IAAA,UAhBA,sBAgBA,MAhBA,EAgBA;AACA,UAAA,MAAA,IAAA,KAAA,EAAA;AACA,aAAA,0BAAA,CAAA,YAAA,GAAA,KAAA,UAAA;AACA,OAFA,MAEA;AACA,aAAA,0BAAA,CAAA,YAAA,GAAA,MAAA;AACA;;AACA,WAAA,eAAA;AACA,KAvBA;AAwBA,IAAA,eAxBA,6BAwBA;AAAA;;AACA,WAAA,iBAAA,GAAA,IAAA;AACA,WAAA,0BAAA,CAAA,OAAA,GAAA,KAAA,aAAA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AACA,MAAA,mBAAA,CACA,qBADA,CACA,YADA,EACA,KAAA,0BADA,EAEA,IAFA,CAEA,UAAA,GAAA,EAAA;AACA,QAAA,MAAA,CAAA,UAAA,GAAA,GAAA,CAAA,IAAA,CAAA,IAAA;AACA,QAAA,MAAA,CAAA,UAAA,GAAA,GAAA,CAAA,IAAA,CAAA,KAAA;AACA,OALA,EAMA,KANA,CAMA,UAAA,CAAA,EAAA;AACA,QAAA,MAAA,CAAA,UAAA,GAAA,EAAA;AACA,QAAA,MAAA,CAAA,UAAA,GAAA,CAAA;AACA,OATA,EAUA,OAVA,CAUA,YAAA;AACA,QAAA,MAAA,CAAA,iBAAA,GAAA,KAAA;AACA,OAZA;AAaA,KAhDA;AAiDA,IAAA,QAjDA,oBAiDA,EAjDA,EAiDA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA;AAAA,QAAA,IAAA,EAAA,yCAAA;AAAA,QAAA,MAAA,EAAA;AAAA,UAAA,EAAA,EAAA;AAAA;AAAA,OAAA;AACA,KAnDA;AAoDA,IAAA,UApDA,sBAoDA,EApDA,EAoDA;AAAA;;AACA,WAAA,QAAA,CAAA,aAAA,CAAA,KAAA,EAAA,CAAA,uBAAA,CAAA,EAAA;AACA,QAAA,OAAA,EAAA,KAAA,EAAA,CAAA,iBAAA,CADA;AAEA,QAAA,WAAA,EAAA,KAAA,EAAA,CAAA,gBAAA;AAFA,OAAA,EAIA,IAJA,CAIA,UAAA,KAAA,EAAA;AACA,YAAA,KAAA,EAAA;AACA,UAAA,mBAAA,CACA,UADA,CACA,YADA,EACA,EADA,EAEA,IAFA,CAEA,UAAA,GAAA,EAAA;AACA,YAAA,MAAA,CAAA,eAAA;AACA,WAJA,EAKA,KALA,CAKA,UAAA,CAAA,EAAA;AACA,YAAA,OAAA,CAAA,GAAA,CAAA,CAAA;AACA,WAPA;AAQA;AACA,OAfA,EAgBA,KAhBA,CAgBA,UAAA,GAAA,EAAA,CACA;AACA,OAlBA;AAmBA;AAxEA,GApEA;;AA8IA;AACA,EAAA,OA/IA,qBA+IA;AACA,SAAA,eAAA;AACA,GAjJA;;AAkJA;AACA;AACA,EAAA,KAAA,EAAA;AACA,uCAAA;AACA,MAAA,OADA,qBACA;AACA,aAAA,eAAA;AACA;AAHA;AADA;AApJA,CAAA","sourcesContent":["<template>\r\n    <div class=\"row\">\r\n        <div class=\"col-12\">\r\n          <div class=\"col-md-12 text-center\">\r\n            <div class=\"h4 mb-4 d-inline-block\">{{ $t('column.coefficient') }}</div>\r\n          </div>\r\n            <div class=\"card\">\r\n                <div class=\"card-body\">\r\n                    <div class=\"row mb-2\">\r\n                        <div class=\"col-sm-8\">\r\n                            <div class=\"search-box me-4 mb-2 d-inline-block\">\r\n                                <div class=\"position-relative\">\r\n                                    <input\r\n                                        v-model=\"searchKeyword\"\r\n                                        type=\"text\"\r\n                                        class=\"form-control\"\r\n                                        @input=\"fetchTableItems\"\r\n                                        :placeholder=\"$t('column.search')\"\r\n                                    />\r\n                                    <i class=\"bx bx-search-alt search-icon\"></i>\r\n                                </div>\r\n                            </div>\r\n                            <span>{{ $t('column.select.text1') }}</span>\r\n                            <div class=\"col-2 me-2 mx-2 d-inline-block\">\r\n                                <div class=\"position-relative\">\r\n                                    <b-form-select\r\n                                        v-model=\"selected\"\r\n                                        :options=\"optionsTable\"\r\n                                        @change=\"selectList\"\r\n                                        class=\"form-select\"\r\n                                    ></b-form-select>\r\n                                </div>\r\n                            </div>\r\n                            <span>{{ $t('column.select.text2') }}</span>\r\n                        </div>\r\n                        <div class=\"col-sm-4\">\r\n                            <div class=\"text-sm-end\">\r\n                                <b-btn\r\n                                    type=\"button\"\r\n                                    class=\"btn btn-success btn-rounded mb-2 me-2\"\r\n                                    :to=\"{name: 'CreateAdvertisementPrivilegeCoefficient'}\"\r\n                                >\r\n                                    <i class=\"mdi mdi-plus me-1\"></i> {{ $t('actions.add') }}\r\n                                </b-btn>\r\n                            </div>\r\n                        </div>\r\n                        <!-- end col-->\r\n                    </div>\r\n                    <!-- Table data -->\r\n                    <b-table\r\n                        :items=\"tableItems\"\r\n                        :fields=\"tableFields\"\r\n                        :busy=\"loadingTableItems\"\r\n                        id=\"my-table\"\r\n                        class=\"custom-b-table\"\r\n                        responsive\r\n                        bordered\r\n                        small\r\n                        hover\r\n                        show-empty\r\n                    >\r\n                        <!-- NUMBER OF ITEM -->\r\n                        <template #cell(index)=\"data\">\r\n                            {{ util_paginate(data.index, var_default_search_payload.page, var_default_search_payload.itemsPerPage) }}\r\n                        </template>\r\n\r\n                        <!-- NAME -->\r\n                        <template #cell(name)=\"data\">\r\n                            <div class=\"d-flex flex-wrap\">\r\n                                <p\r\n                                    class=\"mb-1 d-flex align-items-center\"\r\n                                    style=\"flex: 0 0 100%;\"\r\n                                ><span class=\"badge bg-primary\">ЎЗ</span> : <span> {{ data.item.nameUz }}</span></p>\r\n                                <p\r\n                                    class=\"mb-1 d-flex align-items-center\"\r\n                                    style=\"flex: 0 0 100%;\"\r\n                                ><span class=\"badge bg-primary\">O'Z</span> : <span> {{ data.item.nameLt }}</span></p>\r\n                                <p\r\n                                    class=\"mb-0 d-flex align-items-center\"\r\n                                    style=\"flex: 0 0 100%;\"\r\n                                ><span class=\"badge bg-primary\">РУ</span> : <span> {{ data.item.nameRu }}</span></p>\r\n                            </div>\r\n                        </template>\r\n\r\n                        <!-- AD_DESIGN_TYPES -->\r\n                        <template #cell(adDesignTypes)=\"data\">\r\n                            <div class=\"d-flex align-items-center flex-wrap\">\r\n                                <ul>\r\n                                    <li\r\n                                        v-for=\"(design, index) in data.item.directoryAdvertisementDesignTypes\"\r\n                                        :key=\"`location-design-${index}`\"\r\n                                    >{{\r\n                                        getName({\r\n                                          nameRu: design.nameRu,\r\n                                          nameLt: design.nameLt,\r\n                                          nameUz: design.nameUz,\r\n                                        })\r\n                                      }}</li>\r\n                                </ul>\r\n                            </div>\r\n                        </template>\r\n\r\n                        <!-- STATUS -->\r\n                        <template #cell(status)=\"data\">\r\n                            <p>\r\n                                {{\r\n                  getName({\r\n                    nameRu: data.item.statusNameRu,\r\n                    nameLt: data.item.statusNameLt,\r\n                    nameUz: data.item.statusNameUz,\r\n                  })\r\n                }}\r\n                            </p>\r\n                        </template>\r\n\r\n                        <!-- ACTIONS -->\r\n                        <template #cell(actions)=\"data\">\r\n                            <div class=\"general-table__actions d-flex justify-content-center\">\r\n                                <!-- <b-btn\r\n                                    variant=\"link\"\r\n                                    class=\"text-decoration-none p-0\"\r\n                                    style=\"font-size: 1.2rem; margin-right: 1rem;\"\r\n                                >\r\n                                    <i\r\n                                        @click=\"editItem(data.item.id)\"\r\n                                        class=\"mdi mdi-circle-edit-outline edit\"\r\n                                    ></i>\r\n                                </b-btn> -->\r\n\r\n                                <b-btn\r\n                                    v-if=\"data.item.statusNameEn == 'ACTIVE'\"\r\n                                    variant=\"link\"\r\n                                    class=\"text-decoration-none p-0 text-danger\"\r\n                                    style=\"font-size: 1.2rem;\"\r\n                                >\r\n                                    <i\r\n                                        @click=\"deleteItem(data.item.id)\"\r\n                                        class=\"mdi mdi-trash-can delete\"\r\n                                    ></i>\r\n                                </b-btn>\r\n                            </div>\r\n                        </template>\r\n\r\n                        <!-- EMPTY SLOT -->\r\n                        <template #empty=\"\">\r\n                            <h4 class=\"text-center\">{{ $t('messages.data_not_found') }}</h4>\r\n                        </template>\r\n\r\n                        <!-- TABLE_BUSY SLOT -->\r\n                        <template #table-busy>\r\n                            <div class=\"text-center my-2\">\r\n                                <b-spinner variant=\"primary\" class=\"align-middle\"></b-spinner>\r\n                                \r\n                            </div>\r\n                        </template>\r\n                    </b-table>\r\n\r\n                    <!-- end table -->\r\n                    <b-pagination\r\n                        v-model=\"var_default_search_payload.page\"\r\n                        :total-rows=\"totalItems\"\r\n                        :per-page=\"var_default_search_payload.itemsPerPage\"\r\n                        aria-controls=\"my-table\"\r\n                        class=\"justify-content-end\"\r\n                    ></b-pagination>\r\n                </div>\r\n                <!-- end card-body -->\r\n            </div>\r\n            <!-- end card -->\r\n        </div>\r\n        <!-- end col -->\r\n    </div>\r\n    <!-- end row -->\r\n</template>\r\n\r\n<script>\r\nimport i18n from \"../../../../../../i18n\";\r\n\r\nconst MAIN_API_URL = 'directory/advertisement-design-type_privilege_coefficients'\r\nimport appConfig from \"@/app.config\";\r\nimport crudAndListsService from '@/shared/services/crud_and_list.service'\r\n\r\nexport default {\r\n    page: {\r\n        title: \"Advertisement Privilege Coefficient\",\r\n        meta: [{ name: \"description\", content: appConfig.description }],\r\n    },\r\n    components: {},\r\n    data () {\r\n        return {\r\n            loadingTableItems: false,\r\n            json_fields: {\r\n                [this.$t('column.name_uz')]: \"nameUz\",\r\n                [this.$t('column.name_lt')]: \"nameLt\",\r\n                [this.$t('column.name_ru')]: \"nameRu\",\r\n                [this.$t('column.ad_design_types')]: \"designTypes\",\r\n            },\r\n            json_data: [],\r\n            json_meta: [\r\n                [\r\n                    {\r\n                        key: \"charset\",\r\n                        value: \"utf-8\",\r\n                    },\r\n                ],\r\n            ],\r\n            searchKeyword: '',\r\n            selected: 20,\r\n            optionsTable: [\r\n                { value: 20, text: 20 },\r\n                { value: 50, text: 50 },\r\n                { value: 100, text: 100 },\r\n                { value: 150, text: 150 },\r\n                { value: 200, text: 200 },\r\n                { value: 'all', text: i18n.t('column.all') },\r\n            ],\r\n            title: \"Departments\",\r\n            activeDep: {},\r\n            tableItems: [],\r\n            totalItems: 0,\r\n            tableFields: [\r\n                {\r\n                    label: \"#\",\r\n                    thClass: \"text-center\",\r\n                    tdClass: \"text-center\",\r\n                    sortable: false,\r\n                    key: \"index\",\r\n                },\r\n                { label: this.$t('column.coefficient'), key: \"coefficient\" },\r\n                { label: this.$t('column.ad_design_types'), key: \"adDesignTypes\" },\r\n                { label: this.$t('column.decision_number'), key: \"decisionNumber\" },\r\n                { label: this.$t('column.status'), key: \"status\" },\r\n                { label: this.$t('column.reason'), key: \"description\" },\r\n                {\r\n                    label: this.$t('column.actions'),\r\n                    key: \"actions\",\r\n                    thClass: \"text-center\",\r\n                    tdClass: \"text-center\",\r\n                    sortable: false\r\n                },\r\n            ],\r\n        };\r\n    },\r\n    /*\r\n    COMPUTED */\r\n    computed: {\r\n        numberOfPages () {\r\n            return (!this.totalItems || this.totalItems <= 0 || this.totalItems < this.var_default_search_payload.itemsPerPage) ? 1 : this.totalItems / this.var_default_search_payload.itemsPerPage\r\n        }\r\n    },\r\n    methods: {\r\n        downloadExcel () {\r\n            this.json_data = []\r\n            this.tableItems.forEach(res => {\r\n                let obj = {\r\n                    designTypes: []\r\n                }\r\n                obj.nameUz = res.nameUz\r\n                obj.nameLt = res.nameLt\r\n                obj.nameRu = res.nameRu\r\n                res.designTypes.forEach(t => {\r\n                    obj.designTypes.push(t.nameUz)\r\n                })\r\n                this.json_data.push(obj)\r\n            })\r\n        },\r\n        selectList ($event) {\r\n            if ($event == 'all') {\r\n                this.var_default_search_payload.itemsPerPage = this.totalItems\r\n            } else {\r\n                this.var_default_search_payload.itemsPerPage = $event\r\n            }\r\n            this.fetchTableItems();\r\n        },\r\n        fetchTableItems () {\r\n            this.loadingTableItems = true\r\n            this.var_default_search_payload.keyword = this.searchKeyword\r\n            /* this.var_default_search_payload.sortDesc.push(false)\r\n            if (i18n.locale == 'uz') {\r\n              this.var_default_search_payload.sortBy.push('nameLt')\r\n            } else if (i18n.locale == 'ru') {\r\n              this.var_default_search_payload.sortBy.push('nameRu')\r\n            } else if (i18n.locale == 'uzCyrillic') {\r\n              this.var_default_search_payload.sortBy.push('nameUz')\r\n            } */\r\n            crudAndListsService\r\n                .searchListWithKeyword(MAIN_API_URL, this.var_default_search_payload)\r\n                .then((res) => {\r\n                    this.tableItems = res.data.list;\r\n                    this.totalItems = res.data.total;\r\n                })\r\n                .catch(e => {\r\n                    this.tableItems = [];\r\n                    this.totalItems = 0;\r\n                })\r\n                .finally(() => {\r\n                    this.loadingTableItems = false\r\n                })\r\n        },\r\n        editItem (id) {\r\n            this.$router.push({ name: 'UpdateAdvertisementPrivilegeCoefficient', params: { id: id } })\r\n        },\r\n        deleteItem (id) {\r\n            this.$bvModal.msgBoxConfirm(this.$t('messages.delete_title'), {\r\n                okTitle: this.$t('actions.confirm'),\r\n                cancelTitle: this.$t('actions.cancel')\r\n            })\r\n                .then(value => {\r\n                    if (value) {\r\n                        crudAndListsService\r\n                            .deleteById(MAIN_API_URL, id)\r\n                            .then((res) => {\r\n                                this.fetchTableItems()\r\n                            })\r\n                            .catch(e => {\r\n                                console.log(e)\r\n                            })\r\n                    }\r\n                })\r\n                .catch(err => {\r\n                    // An error occurred\r\n                })\r\n        },\r\n    },\r\n    /* CREATED */\r\n    created () {\r\n        this.fetchTableItems()\r\n    },\r\n    /*\r\n    WATCH */\r\n    watch: {\r\n        'var_default_search_payload.page': {\r\n            handler () {\r\n                this.fetchTableItems()\r\n            }\r\n        }\r\n    }\r\n};\r\n</script>\r\n\r\n<style scoped lang='scss'>\r\n</style>"],"sourceRoot":"src/modules/references/modules/advertisement/modules/ad_privilige_coefficient_for_design_type"}]}