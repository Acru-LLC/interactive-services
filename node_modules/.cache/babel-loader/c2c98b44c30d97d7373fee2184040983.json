AA,CAAA,UAAA,CAAA;AAAA,eAAA,CAAA,CAAA,EAAA,IAAA,IAAA,CAAA,EAAA;AAAA,OAAA,CAAA;AACA,aAAA,KAAA;AACA;AAfA,GAbA;AA8BA,EAAA,IA9BA,kBA8BA;AACA,WAAA;AACA,MAAA,cAAA,EAAA;AADA,KAAA;AAGA,GAlCA;AAmCA,EAAA,KAAA,EAAA;AACA,IAAA,OAAA,EAAA;AACA,MAAA,OAAA,EAAA,KADA;AAEA,MAAA,IAAA,EAAA;AAFA,KADA;AAKA,IAAA,IAAA,EAAA;AACA,MAAA,OAAA,EAAA,CADA;AAEA,MAAA,IAAA,EAAA;AAFA,KALA;AASA,IAAA,KAAA,EAAA;AACA,MAAA,OAAA,EAAA,EADA;AAEA,MAAA,IAAA,EAAA;AAFA,KATA;AAaA,IAAA,IAAA,EAAA;AACA,MAAA,IAAA,EAAA,KADA;AAEA,MAAA,OAAA,EAAA;AAAA,eAAA,EAAA;AAAA;AAFA,KAbA;AAiBA,IAAA,OAAA,EAAA;AACA,MAAA,IAAA,EAAA,OADA;AAEA,MAAA,OAAA,EAAA;AAFA;AAjBA,GAnCA;AAyDA,EAAA,UAAA,EAAA;AACA,IAAA,EAAA,EAAA;AADA;AAzDA,CAAA","sourcesContent":["<script>\r\nimport Tr from \"./tr\";\r\nexport default {\r\n  watch: {\r\n    selectedColumn: {\r\n      handler(v) {\r\n        this.$emit(\"setColumns\", v);\r\n      },\r\n    },\r\n    page:{\r\n       handler(v) {\r\n        this.$emit(\"changePage\", v);\r\n      },\r\n    }\r\n  },\r\n  methods: {\r\n    pushData(data) {\r\n      let found = this.selectedColumn.findIndex((e) => e.id == data.id);\r\n      if (found > -1) {\r\n        this.selectedColumn.splice(found, 1);\r\n      } else {\r\n        this.selectedColumn.push(data);\r\n      }\r\n    },\r\n    reset(array = []) {\r\n      this.selectedColumn = array;\r\n    },\r\n    getClass(data) {\r\n      let found = this.selectedColumn.find((e) => e.id == data.id);\r\n      return found;\r\n    },\r\n  },\r\n  data() {\r\n    return {\r\n      selectedColumn: [],\r\n    };\r\n  },\r\n  props: {\r\n    sidebar: {\r\n      default: false,\r\n      type: Boolean,\r\n    },\r\n    page: {\r\n      default: 1,\r\n      type: Number,\r\n    },\r\n    limit: {\r\n      default: 20,\r\n      type: Number,\r\n    },\r\n    list: {\r\n      type: Array,\r\n      default: () => [],\r\n    },\r\n    loading: {\r\n      type: Boolean,\r\n      default: false,\r\n    },\r\n  },\r\n  components: {\r\n    Tr,\r\n  },\r\n};\r\n</script>\r\n\r\n<template>\r\n  <div>\r\n    <div class=\"table-responsive mb-0\">\r\n      <b-overlay :opacity=\"0.1\" :show=\"loading\" rounded=\"sm\">\r\n        <table class=\"table table-centered table-custom-bordered-column\">\r\n          <thead class=\"thead-light\">\r\n            <slot name=\"thead\" />\r\n          </thead>\r\n          <tbody>\r\n            <template v-for=\"(data, index) in list\">\r\n              <Tr\r\n                @pushData=\"pushData\"\r\n                :sidebar=\"sidebar\"\r\n                :index=\"index\"\r\n                :key=\"data.id + 'Column'\"\r\n                class=\"effect\"\r\n                :class=\"getClass(data) ? 'table-primary' : '' \"\r\n                :data=\"data\"\r\n              >\r\n                <template v-slot:index>\r\n                  <td\r\n                    class=\"text-center p_cursor\"\r\n                    @click.prevent=\"pushData(data)\"\r\n                  >\r\n                    <strong>{{ util_paginate(index, limit+1, page - 1) }}</strong>\r\n                  </td>\r\n                </template>\r\n                <template v-slot:actions>\r\n                  <td class=\"text-center\" v-if=\"!sidebar\">\r\n                    <i\r\n                      v-if=\"data.isEditable\"\r\n                      class=\"\r\n                        bx bx-edit\r\n                        font-size-18\r\n                        p_cursor\r\n                        text-hover-primary\r\n                      \"\r\n                      @click=\"$emit('showModal', 'edit', data)\"\r\n                    ></i>\r\n                    <i\r\n                      v-if=\"data.isEditable\"\r\n                      class=\"\r\n                        bx bx-trash\r\n                        ml-2\r\n                        font-size-18\r\n                        p_cursor\r\n                        text-hover-danger\r\n                      \"\r\n                      @click=\"$emit('showModal', 'delete', data)\"\r\n                    ></i>\r\n                  </td>\r\n                </template>\r\n              </Tr>\r\n            </template>\r\n            <tr class=\"text-center\" v-if=\"list.length == 0\">\r\n              <td\r\n                colspan=\"9\"\r\n                :style=\"loading ? 'height:50px' : 'height:40px'\"\r\n                style=\"vertical-align: middle\"\r\n              >\r\n                <h6 class=\"m-0\">\r\n                  {{ $t(\"messages.data_not_found\") }}\r\n                </h6>\r\n              </td>\r\n            </tr>\r\n          </tbody>\r\n        </table>\r\n      </b-overlay>\r\n    </div>\r\n    <slot name=\"pagination\" />\r\n  </div>\r\n  <!-- end table -->\r\n</template>\r\n\r\n<style lang=\"css\" scoped>\r\n.effect:hover {\r\n  color: #3455f1;\r\n}\r\n</style>\r\n\r\n\r\n"],"sourceRoot":"src/modules/report/columns/components"}]}