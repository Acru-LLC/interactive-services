;AACA,MAAA,aAAA,CAAA,gBAAA,CAAA;AACA,QAAA,SAAA,EAAA,KAAA,MAAA,CAAA,SAAA,IAAA,IADA;AAEA,QAAA,IAAA,EAAA,KAAA,MAAA,CAAA,IAFA;AAGA,QAAA,KAAA,EAAA,KAAA,MAAA,CAAA,KAAA,GAAA,UAAA,CAAA,KAAA,MAAA,CAAA,KAAA,CAAA,GAAA,IAHA;AAIA,QAAA,WAAA,EAAA,KAAA,MAAA,CAAA,WAAA,GAAA,QAAA,CAAA,KAAA,MAAA,CAAA,WAAA,CAAA,GAAA,IAJA;AAKA,QAAA,WAAA,EAAA,KAAA,MAAA,CAAA,WAAA,IAAA,IALA;AAMA,QAAA,UAAA,EAAA,KAAA,MAAA,CAAA,UAAA,IAAA,IANA;AAOA,QAAA,IAAA,EAAA,KAAA,MAAA,CAAA,IAPA;AAQA,QAAA,KAAA,EAAA,KAAA,MAAA,CAAA,KAAA,GAAA,QAAA,CAAA,KAAA,MAAA,CAAA,KAAA,CAAA,GAAA;AARA,OAAA,EASA,IATA,EAUA,IAVA,CAUA,UAAA,GAAA,EAAA;AACA,QAAA,KAAA,CAAA,UAAA,GAAA,GAAA,CAAA,IAAA,CAAA,IAAA;AACA,QAAA,KAAA,CAAA,UAAA,GAAA,GAAA,CAAA,IAAA,CAAA,KAAA;AACA,OAbA,EAcA,KAdA,CAcA,UAAA,CAAA,EAAA;AACA,QAAA,KAAA,CAAA,UAAA,GAAA,EAAA;AACA,QAAA,KAAA,CAAA,UAAA,GAAA,CAAA;AACA,OAjBA,EAkBA,OAlBA,CAkBA,YAAA;AACA,QAAA,KAAA,CAAA,iBAAA,GAAA,KAAA;AACA,OApBA;AAqBA,KAnCA;AAoCA,IAAA,mBApCA,iCAoCA;AAAA;;AACA,MAAA,YAAA,CAAA,KAAA,cAAA,CAAA;AACA,WAAA,cAAA,GAAA,UAAA,CAAA,YAAA;AACA,QAAA,MAAA,CAAA,eAAA;AACA,OAFA,EAEA,IAFA,CAAA;AAGA,KAzCA;AA0CA,IAAA,WA1CA,uBA0CA,IA1CA,EA0CA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA;AACA,QAAA,IAAA,EAAA,kCADA;AAEA,QAAA,MAAA,EAAA;AACA,UAAA,EAAA,EAAA,IAAA,CAAA;AADA;AAFA,OAAA;AAMA;AAjDA,GA5EA;;AA+HA;AACA,EAAA,OAhIA,qBAgIA;AACA,SAAA,eAAA;AACA,GAlIA;;AAmIA;AACA,EAAA,KAAA,EAAA;AACA,uCAAA;AACA,MAAA,OADA,qBACA;AACA,aAAA,eAAA;AACA;AAHA,KADA,CAMA;AACA;AACA;AACA;AACA;AACA;;AAXA;AApIA,CAAA","sourcesContent":["<template>\r\n  <div class=\"row\">\r\n    <div class=\"col-12\">\r\n      <div class=\"col-md-12 text-center\">\r\n        <div class=\"h4 mb-4 d-inline-block\">{{ $t('submodules.integration.statistics_info.title') }}</div>\r\n      </div>\r\n      <div class=\"card\">\r\n        <div class=\"card-body\">\r\n          <div class=\"row mb-2\">\r\n            <div class=\"col-sm-8\">\r\n              <!--              <div class=\"search-box me-4 mb-2 d-inline-block\">\r\n                              <div class=\"position-relative\">\r\n                                <input\r\n                                    v-model=\"searchKeyword\"\r\n                                    :placeholder=\"$t('column.search')\"\r\n                                    class=\"form-control\"\r\n                                    type=\"text\"\r\n                                    @input=\"changeSearchKeyword\"\r\n                                />\r\n                                <i class=\"bx bx-search-alt search-icon\"></i>\r\n                              </div>\r\n                            </div>\r\n                            <span>{{ $t('column.select.text1') }}</span>-->\r\n              <!--              <div class=\"col-2 me-2 mx-2 d-inline-block\">\r\n                              <div class=\"position-relative\">\r\n                                <b-form-select\r\n                                    v-model=\"params.size\"\r\n                                    :options=\"optionsTable\"\r\n                                    class=\"form-select\"\r\n                                    @change=\"selectList\"\r\n                                ></b-form-select>\r\n                              </div>\r\n                            </div>\r\n                            <span>{{ $t('column.select.text2') }}</span>\r\n                          </div>-->\r\n              <div class=\"col-sm-4\">\r\n                <div class=\"text-sm-end\">\r\n                  <b-row>\r\n                    <b-col cols=\"5\">\r\n                      <div class=\"text-sm-end\">\r\n                        <download-excel\r\n                            :data=\"json_data\"\r\n                            :fields=\"json_fields\"\r\n                            :header=\"$t('submodules.integration.statistics_info.title')\"\r\n                            :name=\"`${$t('submodules.integration.statistics_info.title')}.xls`\"\r\n                            worksheet=\"My Worksheet\"\r\n                        >\r\n                          <b-btn\r\n                              class=\"btn btn-rounded bg-primary mb-2\"\r\n                              type=\"button\"\r\n                              @click=\"downloadExcel\"\r\n                          >\r\n                            <i class=\"mdi mdi-microsoft-excel me-1\"></i> {{ $t('actions.download') }}\r\n                          </b-btn>\r\n                        </download-excel>\r\n                      </div>\r\n                    </b-col>\r\n                  </b-row>\r\n                </div>\r\n              </div>\r\n            </div>\r\n            <b-table\r\n                id=\"my-table\"\r\n                :busy=\"loadingTableItems\"\r\n                :fields=\"tableFields\"\r\n                :items=\"tableItems\"\r\n                bordered\r\n                class=\"custom-b-table\"\r\n                hover\r\n                responsive\r\n                show-empty\r\n                small\r\n                striped\r\n            >\r\n              <template #top-row>\r\n                <td></td>\r\n                <td>\r\n                  <b-form-input v-model=\"params.soato\" type=\"number\" @keyup=\"changeSearchKeyword\"/>\r\n                </td>\r\n                <td>\r\n                  <b-form-input v-model=\"params.regionName\" @keyup=\"changeSearchKeyword\"/>\r\n                </td>\r\n                <td>\r\n                  <b-form-input v-model=\"params.productCode\" type=\"number\" @keyup=\"changeSearchKeyword\"/>\r\n                </td>\r\n                <td>\r\n                  <b-form-input v-model=\"params.productName\" @keyup=\"changeSearchKeyword\"/>\r\n                </td>\r\n                <td>\r\n                  <b-form-input v-model=\"params.price\" type=\"number\" @keyup=\"changeSearchKeyword\"/>\r\n                </td>\r\n                <td>\r\n                  <BaseDatePickerWithValidation\r\n                    v-model=\"params.interDate\"\r\n                    only-form-element\r\n                    @change=\"changeSearchKeyword\"\r\n                  />\r\n                </td>\r\n              </template>\r\n              <template #cell(index)=\"data\">\r\n                {{ util_paginate(data.index, params.page, params.size) }}\r\n              </template>\r\n              <template #cell(name)=\"data\">\r\n                <div class=\"d-flex justify-content-between\">\r\n                  <p\r\n                      class=\"mb-0 d-flex align-items-center\"\r\n                      style=\"flex-basis: 0; flex-grow: 1; gap: .3rem;\"\r\n                  ><span class=\"badge bg-primary\">ЎЗ</span> : <span> {{ data.item.nameUz }}</span></p>\r\n                  <p\r\n                      class=\"mb-0 d-flex align-items-center\"\r\n                      style=\"flex-basis: 0; flex-grow: 1; gap: .3rem;\"\r\n                  ><span class=\"badge bg-primary\">O'Z</span> &nbsp;:&nbsp; <span> {{ data.item.nameLt }}</span></p>\r\n                  <p\r\n                      class=\"mb-0 d-flex align-items-center\"\r\n                      style=\"flex-basis: 0; flex-grow: 1; gap: .3rem;\"\r\n                  ><span class=\"badge bg-primary\">РУ</span> &nbsp;:&nbsp; <span> {{ data.item.nameRu }}</span></p>\r\n                </div>\r\n              </template>\r\n              <template #cell(actions)=\"data\">\r\n                <div class=\"general-table__actions d-flex justify-content-center\">\r\n                  <b-btn\r\n                      class=\"text-decoration-none p-0\"\r\n                      style=\"font-size: 1.2rem; margin-right: 1rem;\"\r\n                      variant=\"link\"\r\n                  >\r\n                    <i class=\"mdi mdi-circle-edit-outline edit\" @click=\"confirmItem(data.item)\"/>\r\n                  </b-btn>\r\n                </div>\r\n              </template>\r\n              <template #empty=\"\">\r\n                <h4 class=\"text-center\">{{ $t('messages.data_not_found') }}</h4>\r\n              </template>\r\n              <template #table-busy>\r\n                <div class=\"text-center my-2\">\r\n                  <b-spinner class=\"align-middle\" variant=\"primary\"></b-spinner>\r\n                </div>\r\n              </template>\r\n            </b-table>\r\n            <b-pagination\r\n                v-model=\"var_default_search_payload.page\"\r\n                :per-page=\"var_default_search_payload.itemsPerPage\"\r\n                :total-rows=\"totalItems\"\r\n                aria-controls=\"my-table\"\r\n                class=\"justify-content-end\"\r\n            ></b-pagination>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nconst MAIN_API_URL = '/statistics-info'\r\nconst INTEGRATION_API_URL = '/statistics-info/deploy-information'\r\nimport helperService from '@/shared/services/helper.service'\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      loadingTableItems: false,\r\n      json_fields: {\r\n        'ID': 'id',\r\n        [this.$t('submodules.integration.statistics_info.saoto')]: 'soato',\r\n        [this.$t('submodules.integration.statistics_info.saoto_name')]: 'regionName',\r\n        [this.$t('submodules.integration.statistics_info.code_tovar')]: 'productCode',\r\n        [this.$t('submodules.integration.statistics_info.tovar')]: 'productName',\r\n        [this.$t('submodules.integration.statistics_info.price')]: 'price',\r\n        [this.$t('submodules.integration.statistics_info.date_inter')]: 'interDate',\r\n      },\r\n      json_data: [],\r\n      json_meta: [\r\n        [\r\n          {\r\n            key: \"charset\",\r\n            value: \"utf-8\",\r\n          },\r\n        ],\r\n      ],\r\n      searchKeyword: '',\r\n      optionsTable: [\r\n        { value: 20, text: 20 },\r\n        { value: 50, text: 50 },\r\n        { value: 100, text: 100 },\r\n        { value: 150, text: 150 },\r\n        { value: 200, text: 200 },\r\n        { value: 'all', text: this.$t('column.all') },\r\n      ],\r\n      activeDep: {},\r\n      tableItems: [],\r\n      totalItems: 0,\r\n      tableFields: [\r\n        {\r\n          label: \"#\",\r\n          thClass: \"text-center\",\r\n          tdClass: \"text-center\",\r\n          sortable: false,\r\n          key: \"index\",\r\n        },\r\n        { label: this.$t('submodules.integration.statistics_info.soato'), key: \"soato\" },\r\n        { label: this.$t('submodules.integration.statistics_info.saoto_name'), key: \"regionName\" },\r\n        { label: this.$t('submodules.integration.statistics_info.code_tovar'), key: \"productCode\" },\r\n        { label: this.$t('submodules.integration.statistics_info.tovar'), key: \"productName\" },\r\n        { label: this.$t('submodules.integration.statistics_info.price'), key: \"price\" },\r\n        { label: this.$t('submodules.integration.statistics_info.date_inter'), key: \"interDate\" },\r\n        // {\r\n        //   label: this.$t('column.actions'),\r\n        //   key: \"actions\",\r\n        //   thClass: \"text-center\",\r\n        //   tdClass: \"text-center\",\r\n        //   sortable: false\r\n        // },\r\n      ],\r\n      searchInterval: null,\r\n      params: {\r\n        page: 1,\r\n        size: 100,\r\n        price: null,\r\n        productName: null,\r\n        productCode: null,\r\n        soato: null,\r\n        interDate: null,\r\n        regionName: null,\r\n      }\r\n    };\r\n  },\r\n  /*\r\n  COMPUTED */\r\n  computed: {\r\n    numberOfPages() {\r\n      return (!this.totalItems || this.totalItems <= 0 || this.totalItems < this.var_default_search_payload.itemsPerPage) ? 1 : this.totalItems / this.var_default_search_payload.itemsPerPage\r\n    }\r\n  },\r\n  methods: {\r\n    downloadExcel() {\r\n      this.json_data = this.tableItems\r\n    },\r\n    selectList($event) {\r\n      if ($event === 'all') {\r\n        this.var_default_search_payload.itemsPerPage = this.totalItems\r\n      } else {\r\n        this.var_default_search_payload.itemsPerPage = $event\r\n      }\r\n      this.fetchTableItems();\r\n    },\r\n    fetchTableItems() {\r\n      this.loadingTableItems = true\r\n      helperService.getStatisticInfo({\r\n        interDate: this.params.interDate || null,\r\n        page: this.params.page,\r\n        price: this.params.price ? parseFloat(this.params.price) : null,\r\n        productCode: this.params.productCode ? parseInt(this.params.productCode) : null,\r\n        productName: this.params.productName || null,\r\n        regionName: this.params.regionName || null,\r\n        size: this.params.size,\r\n        soato: this.params.soato ? parseInt(this.params.soato) : null,\r\n      }, true)\r\n          .then((res) => {\r\n            this.tableItems = res.data.data;\r\n            this.totalItems = res.data.count;\r\n          })\r\n          .catch(e => {\r\n            this.tableItems = [];\r\n            this.totalItems = 0;\r\n          })\r\n          .finally(() => {\r\n            this.loadingTableItems = false\r\n          })\r\n    },\r\n    changeSearchKeyword() {\r\n      clearTimeout(this.searchInterval)\r\n      this.searchInterval = setTimeout(() => {\r\n        this.fetchTableItems()\r\n      }, 1000)\r\n    },\r\n    confirmItem(item) {\r\n      this.$router.push({\r\n        name: 'IntegrationStatisticsInfoConfirm',\r\n        params: {\r\n          id: item.id\r\n        }\r\n      })\r\n    },\r\n  },\r\n  /* CREATED */\r\n  created() {\r\n    this.fetchTableItems()\r\n  },\r\n  /* WATCH */\r\n  watch: {\r\n    'var_default_search_payload.page': {\r\n      handler() {\r\n        this.fetchTableItems()\r\n      },\r\n    },\r\n    // params: {\r\n    //   handler() {\r\n    //     this.fetchTableItems()\r\n    //   },\r\n    //   deep: true\r\n    // },\r\n  }\r\n};\r\n</script>\r\n\r\n<style lang='scss' scoped>\r\n</style>"],"sourceRoot":"src/modules/integration/modules/statistics"}]}