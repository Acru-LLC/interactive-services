\n    <td class=\"text-center\">\r\n      <b-form-checkbox switch v-model=\"data.isVertical\" :value=\"1\" :unchecked-value=\"0\">\r\n\r\n      </b-form-checkbox>\r\n    </td>\r\n    <td @click=\"$emit('pushData', data)\" v-if=\"!sidebar\" class=\"text-center\">\r\n      {{ data.typeCode }}\r\n    </td>\r\n    <td @click=\"$emit('pushData', data)\" v-if=\"!sidebar\" class=\"text-center\">\r\n      {{ data.comment }}\r\n    </td>\r\n    <td @click=\"$emit('pushData', data)\" v-if=\"!sidebar\" class=\"text-center\">\r\n      {{ data.createdDate }}\r\n    </td>\r\n\r\n    <slot name=\"actions\"></slot>\r\n  </tr>\r\n\r\n</template>\r\n\r\n<script>\r\n// import { getName } from \"@/helper\";\r\nexport default {\r\n  data() {\r\n    return {\r\n      ischeck: false,\r\n      // getName: getName,\r\n    };\r\n  },\r\n  methods: {\r\n    getStyle(data) {\r\n      var total = 0;\r\n      data.forEach( (element) => {\r\n        total = element.children.length;\r\n      } );\r\n      let i = 0;\r\n      if (data.length > 0) {\r\n        while (i <= data.length) {\r\n          if (i % 2 == 0) {\r\n            i++;\r\n            return `height:${data.length + total}0pt`;\r\n          } else {\r\n            i++;\r\n            return \"\";\r\n          }\r\n        }\r\n      }\r\n    },\r\n  },\r\n  props: [\"data\", \"sidebar\"],\r\n  watch: {\r\n    'data.isVertical': {\r\n      handler() {\r\n        this.$emit('isVerticalChange', this.data)\r\n      }\r\n    }\r\n  }\r\n}\r\n</script>\r\n<style scoped>\r\n.rotate_text {\r\n  height: 0;\r\n  writing-mode: vertical-lr;\r\n  -webkit-writing-mode: vertical-lr;\r\n  -ms-writing-mode: vertical-lr;\r\n  -webkit-transform: rotate(-180deg);\r\n  -moz-transform: rotate(-180deg);\r\n  -o-transform: rotate(-180deg);\r\n  transform: rotate(-180deg);\r\n  margin: auto;\r\n}\r\n</style>\r\n"]}]}