d=\"contact-list\"\r\n            ref=\"contactsRef\"\r\n        >\r\n            <ul class=\"list-unstyled chat-list\">\r\n                <li\r\n                    :class=\"{ active: username == `${data.firstName} ${data.lastName}` }\"\r\n                    :id=\"contactList[contactList.length - 1].id == data.id ? 'lid' : ''\"\r\n                    v-for=\"(data, index) in contactList\"\r\n                    :key=\"data.employeeId + 'GROUP_MEMBER_INDEX' + index\"\r\n                >\r\n                    <a href=\"javascript: void(0);\">\r\n                        <div\r\n                            class=\"media d-flex\"\r\n                            style=\"align-items: center\"\r\n                        >\r\n                            <div class=\"align-self-center mr-3\"></div>\r\n                            <div\r\n                                class=\"align-self-center mr-3\"\r\n                                v-if=\"data.uploadPath\"\r\n                            >\r\n                                <img\r\n                                    @click=\"\r\n                    () => {\r\n                      uploadPath = data.uploadPath;\r\n                      singleImage = true;\r\n                    }\r\n                  \"\r\n                                    :src=\"`${publicPath}${data.uploadPath}`\"\r\n                                    class=\"rounded-circle avatar-sm\"\r\n                                    alt\r\n                                />\r\n                            </div>\r\n                            <div\r\n                                class=\"avatar-sm align-self-center mr-3\"\r\n                                v-if=\"!data.uploadPath\"\r\n                            >\r\n                                <span class=\"\r\n                    avatar-title\r\n                    rounded-circle\r\n                    bg-soft-primary\r\n                    text-primary\r\n                  \">{{ data.employeeFullName ? data.employeeFullName.charAt(0) : ''}}</span>\r\n                            </div>\r\n                            <div\r\n                                @click=\"clickMember(data)\"\r\n                                class=\"media-body overflow-hidden align-self-stretch d-flex align-items-center\"\r\n                            >\r\n                                <h5\r\n                                    :class=\"check(data)\"\r\n                                    class=\"font-size-14 mb-1\"\r\n                                >\r\n                                    {{ `${data.employeeFullName}  ` }}\r\n                                </h5>\r\n                                <span :class=\"check(data)\">\r\n                                    {{\r\n                    getName({\r\n                      nameLt: data.depNameLt,\r\n                      nameRu: data.depNameRu,\r\n                      nameUz: data.depNameUz,\r\n                    })\r\n                  }}\r\n                                </span>\r\n\r\n                                <span :class=\"check(data)\">\r\n                                    {{\r\n                    getName({\r\n                      nameLt: data.positionNameLt,\r\n                      nameRu: data.positionNameRu,\r\n                      nameUz: data.positionNameUz,\r\n                    })\r\n                  }}\r\n                                </span>\r\n                            </div>\r\n                            <i\r\n                                v-if=\"check(data)\"\r\n                                class=\"fas fa-user-check font-size-18 text-primary\"\r\n                            ></i>\r\n                        </div>\r\n                    </a>\r\n                </li>\r\n            </ul>\r\n        </simplebar>\r\n        <hr />\r\n        <div class=\"d-flex align-items-center justify-content-between pl-3 pr-3 pb-3\">\r\n            <div></div>\r\n            <div>\r\n                <b-button\r\n                    :disabled=\"members.length == 0\"\r\n                    class=\"ml-1\"\r\n                    @click=\"deleteMember\"\r\n                    variant=\"danger\"\r\n                >\r\n                    <b-overlay\r\n                        :opacity=\"0.1\"\r\n                        :show=\"loader\"\r\n                        rounded=\"sm\"\r\n                    >\r\n                        <b-badge\r\n                            class=\"ml-2\"\r\n                            variant=\"dark\"\r\n                        >{{\r\n              `${members.length}`\r\n            }}</b-badge>\r\n\r\n                        {{ $t(\"actions.remove_from_group\") }}\r\n                        <i class=\"fa fa-trash\"></i>\r\n                    </b-overlay>\r\n                </b-button>\r\n            </div>\r\n        </div>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport messengerService from \"@/shared/services/messengerService\";\r\nimport simplebar from \"simplebar-vue\";\r\nimport VueEasyLightbox from \"vue-easy-lightbox\";\r\n// import { getName } from \"@/helper\";\r\nexport default {\r\n    props: {\r\n        username: {\r\n            type: String,\r\n            default: \"\",\r\n        },\r\n        group: {\r\n            type: Object,\r\n            default: () => { },\r\n        },\r\n        searchValue: {\r\n            type: String,\r\n            default: \"\",\r\n        },\r\n    },\r\n    components: {\r\n        simplebar,\r\n        VueEasyLightbox,\r\n    },\r\n    watch: {\r\n        searchValue (v) {\r\n            this.reset(v);\r\n        },\r\n    },\r\n    data () {\r\n        return {\r\n            publicPath: process.env.BASE_URL,\r\n            // getName: getName,\r\n            uploadPath: \"\",\r\n            contactList: [],\r\n            contactPage: 0,\r\n            singleImage: false,\r\n            cmpKey: 0,\r\n            members: [],\r\n            loader: false,\r\n            totalMember: 0,\r\n        };\r\n    },\r\n    methods: {\r\n        deleteMember () {\r\n            this.loader = true;\r\n            messengerService\r\n                .deleteMemberFromGroup(this.members, this.group.id)\r\n                .then((rs) => {\r\n                    setTimeout(() => {\r\n                        this.members = [];\r\n                        this.getClickTab({ page: 0, itemsPerPage: 30, search: \"\" });\r\n                    }, 300);\r\n                })\r\n                .catch((err) => {\r\n                    // catchErr(err);\r\n                })\r\n                .finally(() => {\r\n                    this.loader = false;\r\n                });\r\n        },\r\n        clickMember (v) {\r\n            let index = this.members.indexOf(v.employeeId);\r\n            if (index > -1) {\r\n                this.members.splice(index, 1);\r\n            } else {\r\n                this.members.unshift(v.employeeId);\r\n            }\r\n        },\r\n        reset (v = \"\") {\r\n            this.cmpKey += 1;\r\n            this.searchValue = v;\r\n            setTimeout(() => {\r\n                this.getClickTab({ page: 0, itemsPerPage: 30, search: v });\r\n                this.scrollActive();\r\n            }, 200);\r\n        },\r\n        scrollActive () {\r\n            if (this.$refs.contactsRef.SimpleBar) {\r\n                this.$refs.contactsRef.SimpleBar.getScrollElement().addEventListener(\r\n                    \"scroll\",\r\n                    (e) => {\r\n                        let { scrollTop, scrollHeight, clientHeight } = event.target;\r\n                        const offsetForTrigger = 1;\r\n                        if (scrollHeight - clientHeight - offsetForTrigger < scrollTop) {\r\n                            this.loadMoreContact();\r\n                        }\r\n                    }\r\n                );\r\n            }\r\n        },\r\n        check (v) {\r\n            let index = this.members.indexOf(v.employeeId);\r\n            if (index > -1) {\r\n                return [\"font-weight-bold\", \"text-primary\"];\r\n            } else {\r\n                return false;\r\n            }\r\n        },\r\n        getClickTab (v) {\r\n            this.contactPage = 0;\r\n            this.cmpKey += 1;\r\n            this.getContacts(v);\r\n        },\r\n        getContacts (v) {\r\n            messengerService\r\n                .groupMembersById(this.group.id, v)\r\n                .then(({ data }) => {\r\n                    if (data) {\r\n                        this.$nextTick(() => {\r\n                            this.contactList = data.list;\r\n                            this.totalMember = data.total;\r\n                            this.$emit(\"setTotalForGroupMember\", data);\r\n                        });\r\n                    }\r\n                })\r\n                .catch((err) => {\r\n                    // catchErr(err);\r\n                });\r\n        },\r\n        loadMoreContact () {\r\n            messengerService\r\n                .groupMembersById(this.group.id, {\r\n                    page: (this.contactPage += 1),\r\n                    itemsPerPage: 20,\r\n                    search: this.searchValue,\r\n                })\r\n                .then(async ({ data }) => {\r\n                    if (data.list.length > 0) {\r\n                        let contactList = data.list;\r\n                        this.$nextTick(() => {\r\n                            this.contactList = this.contactList.concat(contactList);\r\n                        });\r\n                    } else {\r\n                        this.contactPage = this.contactPage - 1;\r\n                    }\r\n                })\r\n                .catch((err) => {\r\n                    // catchErr(err);\r\n                });\r\n        },\r\n    },\r\n    mounted () {\r\n        var container3 = document.querySelector(\r\n            \"#contact-list .simplebar-content-wrapper\"\r\n        );\r\n        if (container3) {\r\n            container3.scrollTo({ top: 500, behavior: \"smooth\" });\r\n        }\r\n        // Contact list actions\r\n        this.$refs.contactsRef.SimpleBar.getScrollElement().addEventListener(\r\n            \"scroll\",\r\n            (e) => {\r\n                let { scrollTop, scrollHeight, clientHeight } = event.target;\r\n                const offsetForTrigger = 1;\r\n                if (scrollHeight - clientHeight - offsetForTrigger < scrollTop) {\r\n                    this.loadMoreContact();\r\n                }\r\n            }\r\n        );\r\n    },\r\n};\r\n</script>\r\n\r\n<style>\r\n</style>"],"sourceRoot":"src/modules/chat"}]}