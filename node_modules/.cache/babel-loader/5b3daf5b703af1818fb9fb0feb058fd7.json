AA,CAAA,CAAA,EAAA;AACA,aAAA,OAAA,CAAA,MAAA,CAAA,KAAA,EAAA,CAAA;AACA,aAAA,aAAA,CAAA,MAAA,CAAA,KAAA,EAAA,CAAA;AACA,OAHA,MAGA;AACA,aAAA,OAAA,CAAA,OAAA,CAAA,CAAA,CAAA,EAAA;AACA,aAAA,aAAA,CAAA,OAAA,CAAA,CAAA;AACA;AACA,KAlBA;AAmBA,IAAA,gBAnBA,8BAmBA;AACA,WAAA,aAAA,GAAA,EAAA;AACA,KArBA;AAsBA,IAAA,OAtBA,mBAsBA,IAtBA,EAsBA;AACA,WAAA,aAAA,GAAA,IAAA;AACA,WAAA,KAAA,CAAA,SAAA,EAAA,IAAA;AACA,KAzBA;AA0BA,IAAA,QA1BA,oBA0BA,IA1BA,EA0BA;AACA,UAAA,KAAA,GAAA,KAAA,WAAA,CAAA,SAAA,CAAA,UAAA,CAAA;AAAA,eAAA,CAAA,CAAA,EAAA,IAAA,IAAA,CAAA,EAAA;AAAA,OAAA,CAAA;;AACA,UAAA,KAAA,GAAA,CAAA,CAAA,EAAA;AACA,aAAA,WAAA,CAAA,MAAA,CAAA,KAAA,EAAA,CAAA;AACA,OAFA,MAEA;AACA,aAAA,WAAA,CAAA,IAAA,CAAA,IAAA;AACA;AACA,KAjCA;AAkCA,IAAA,KAlCA,mBAkCA;AAAA,UAAA,KAAA,uEAAA,EAAA;AACA,WAAA,WAAA,GAAA,KAAA;AACA;AApCA,GAjBA;AAuDA,EAAA,KAAA,EAAA;AACA,IAAA,OAAA,EAAA;AACA,MAAA,OAAA,EAAA,KADA;AAEA,MAAA,IAAA,EAAA;AAFA,KADA;AAKA,IAAA,IAAA,EAAA;AACA,MAAA,OAAA,EAAA,CADA;AAEA,MAAA,IAAA,EAAA;AAFA,KALA;AASA,IAAA,KAAA,EAAA;AACA,MAAA,OAAA,EAAA,EADA;AAEA,MAAA,IAAA,EAAA;AAFA,KATA;AAaA,IAAA,IAAA,EAAA;AACA,MAAA,IAAA,EAAA,KADA;AAEA,MAAA,OAAA,EAAA;AAAA,eAAA,EAAA;AAAA;AAFA,KAbA;AAiBA,IAAA,OAAA,EAAA;AACA,MAAA,IAAA,EAAA,OADA;AAEA,MAAA,OAAA,EAAA;AAFA;AAjBA,GAvDA;AA6EA,EAAA,UAAA,EAAA;AACA,IAAA,EAAA,EAAA;AADA;AA7EA,CAAA","sourcesContent":["<script>\r\nimport Tr from \"./tr_gather_report\";\r\n// import { getName } from \"@/helper\";\r\nexport default {\r\n  watch: {\r\n    selectedRow: {\r\n      handler(v) {\r\n        this.$emit(\"setRow\", v);\r\n      },\r\n    },\r\n  },\r\n  data() {\r\n    return {\r\n      // getName: getName,\r\n      selectedRow: [],\r\n      currentTrData: {},\r\n      members: [],\r\n      objectMembers: [],\r\n    };\r\n  },\r\n  methods: {\r\n    check(v) {\r\n      let index = this.members.indexOf(v.id);\r\n      if (index > -1) {\r\n        return [\"font-weight-bold\", \"text-success\"];\r\n      } else {\r\n        return false;\r\n      }\r\n    },\r\n    pushMember(v) {\r\n      let index = this.members.indexOf(v.id);\r\n      if (index > -1) {\r\n        this.members.splice(index, 1);\r\n        this.objectMembers.splice(index, 1);\r\n      } else {\r\n        this.members.unshift(v.id);\r\n        this.objectMembers.unshift(v);\r\n      }\r\n    },\r\n    resetCurrentData() {\r\n      this.currentTrData = {};\r\n    },\r\n    clickTr(data) {\r\n      this.currentTrData = data;\r\n      this.$emit(\"clickTr\", data);\r\n    },\r\n    pushData(data) {\r\n      let found = this.selectedRow.findIndex((e) => e.id == data.id);\r\n      if (found > -1) {\r\n        this.selectedRow.splice(found, 1);\r\n      } else {\r\n        this.selectedRow.push(data);\r\n      }\r\n    },\r\n    reset(array = []) {\r\n      this.selectedRow = array;\r\n    },\r\n  },\r\n  props: {\r\n    sidebar: {\r\n      default: false,\r\n      type: Boolean,\r\n    },\r\n    page: {\r\n      default: 1,\r\n      type: Number,\r\n    },\r\n    limit: {\r\n      default: 20,\r\n      type: Number,\r\n    },\r\n    list: {\r\n      type: Array,\r\n      default: () => [],\r\n    },\r\n    loading: {\r\n      type: Boolean,\r\n      default: false,\r\n    },\r\n  },\r\n  components: {\r\n    Tr,\r\n  },\r\n};\r\n</script>\r\n\r\n<template>\r\n  <div>\r\n    <div class=\"table-responsive mb-0\">\r\n      <b-overlay :opacity=\"0.1\" :show=\"loading\" rounded=\"sm\">\r\n        <table\r\n          class=\"table table-centered table-striped table-nowrap table-hover\"\r\n        >\r\n          <thead class=\"thead-light\">\r\n            <slot name=\"thead\" />\r\n          </thead>\r\n          <tbody>\r\n            <template v-for=\"(data, index) in list\">\r\n              <Tr\r\n                @clickTr=\"clickTr\"\r\n                :class=\"\r\n                  currentTrData.id == data.id\r\n                    ? 'bg-soft-primary text-primary'\r\n                    : data.status == 'CANCELED'\r\n                    ? 'bg-soft-danger text-danger'\r\n                    : ''\r\n                \"\r\n                @pushData=\"pushData\"\r\n                :index=\"index\"\r\n                :key=\"data.id + 'c-1'\"\r\n                :data=\"data\"\r\n                :classCheck=\"check(data)\"\r\n              >\r\n                <template v-slot:check>\r\n                  <td class=\"bg-white\">\r\n                    <b-button\r\n                      v-if=\"data.status == 'APPROVED'\"\r\n                      @click.prevent=\"pushMember(data)\"\r\n                      :variant=\"check(data) ? 'success' : 'light'\"\r\n                      size=\"sm\"\r\n                    >\r\n                      <i class=\"fa fa-check\"></i>\r\n                    </b-button>\r\n                  </td>\r\n                </template>\r\n                <template v-slot:index>\r\n                  <td\r\n                    @click=\"clickTr(data)\"\r\n                    class=\"text-center p_cursor\"\r\n                    @click.prevent=\"pushData(data)\"\r\n                  >\r\n                    <strong :class=\"check(data)\">\r\n                      {{ paginate(index, limit, page - 1) }}</strong\r\n                    >\r\n                  </td>\r\n                </template>\r\n\r\n                <template v-slot:name>\r\n                  <td @click=\"clickTr(data)\" class=\"text-center p_cursor\">\r\n                    <span\r\n                      :class=\"check(data)\"\r\n                      class=\"text-hover-primary p_cursor\"\r\n                    >\r\n                      {{\r\n                        getName({\r\n                          nameLt: data.docTableNameLt,\r\n                          nameRu: data.docTableNameRu,\r\n                        })\r\n                      }}\r\n                    </span>\r\n                  </td>\r\n                </template>\r\n                <!-- <template v-slot:actions>\r\n                  <td v-if=\"!sidebar\" class=\"text-center\">\r\n\r\n                    <b-button\r\n                      @click=\"$emit('showModal', 'view', data)\"\r\n                      size=\"sm\"\r\n                      class=\"ml-2\"\r\n                      variant=\"light\"\r\n                    >\r\n                      <i class=\"fa fa-eye font-size-18\"></i>\r\n                    </b-button>\r\n                    <b-button\r\n                      v-if=\"data.status == 'CREATED'\"\r\n                      @click=\"$emit('showModal', 'edit', data)\"\r\n                      size=\"sm\"\r\n                      class=\"ml-2\"\r\n                      variant=\"light\"\r\n                    >\r\n                      <i class=\"bx bx-edit font-size-18 text-hover-primary\"></i>\r\n                    </b-button>\r\n                    <b-button\r\n                      v-if=\"data.status == 'CREATED'\"\r\n                      @click=\"$emit('showModal', 'delete', data)\"\r\n                      size=\"sm\"\r\n                      class=\"ml-2\"\r\n                      variant=\"light\"\r\n                    >\r\n                      <i\r\n                        class=\"\r\n                          bx bx-trash\r\n                          font-size-18\r\n                          p_cursor\r\n                          text-hover-danger\r\n                        \"\r\n                      ></i>\r\n                    </b-button>\r\n\r\n                    <b-button\r\n                      v-if=\"data.status == 'CREATED'\"\r\n                      @click=\"$emit('showModal', 'send', data)\"\r\n                      size=\"sm\"\r\n                      class=\"ml-2\"\r\n                      variant=\"primary\"\r\n                    >\r\n                      <i\r\n                        class=\"\r\n                          bx bx-send\r\n                          font-size-18\r\n                          p_cursor\r\n                          text-hover-danger\r\n                        \"\r\n                      ></i>\r\n                    </b-button>\r\n                  </td>\r\n                </template> -->\r\n              </Tr>\r\n            </template>\r\n            <tr class=\"text-center\" v-if=\"list.length == 0\">\r\n              <td\r\n                colspan=\"9\"\r\n                :style=\"loading ? 'height:50px' : 'height:40px'\"\r\n                style=\"vertical-align: middle\"\r\n              >\r\n                <h6 class=\"m-0\">\r\n                  {{ $t(\"messages.data_not_found\") }}\r\n                </h6>\r\n              </td>\r\n            </tr>\r\n          </tbody>\r\n        </table>\r\n      </b-overlay>\r\n    </div>\r\n    <slot name=\"pagination\" />\r\n  </div>\r\n  <!-- end table -->\r\n</template>\r\n"],"sourceRoot":"src/modules/report/organizitional/components"}]}