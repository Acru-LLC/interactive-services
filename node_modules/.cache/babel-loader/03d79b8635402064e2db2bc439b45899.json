n                      <download-excel\r\n                          :data=\"json_data\"\r\n                          :fields=\"json_fields\"\r\n                          header=\"Минимал йиғим 1 кв/м учун\"\r\n                          worksheet=\"My Worksheet\"\r\n                          name=\"Минимал_йиғим_1_кв/м_учун.xls\"\r\n                      >\r\n                        <b-btn\r\n                            @click=\"downloadExcel\"\r\n                            type=\"button\"\r\n                            class=\"btn btn-rounded bg-primary mb-2\"\r\n                        >\r\n                          <i class=\"mdi mdi-microsoft-excel me-1\"></i> {{ $t('actions.download') }}\r\n                        </b-btn>\r\n                      </download-excel>\r\n                    </div>\r\n                  </b-col>\r\n                  <b-col cols=\"5\">\r\n                    <b-btn\r\n                        type=\"button\"\r\n                        class=\"btn btn-success btn-rounded mb-2 me-2\"\r\n                        :to=\"{name: 'CreateMinimumCollection'}\"\r\n                    >\r\n                      <i class=\"mdi mdi-plus me-1\"></i> {{ $t('actions.add') }}\r\n                    </b-btn>\r\n                  </b-col>\r\n                </b-row>\r\n              </div>\r\n            </div>\r\n            <!-- end col-->\r\n          </div>\r\n\r\n            <b-row class=\"mb-2\">\r\n              <b-col cols=\"4\">\r\n                <!-- REGIONS -->\r\n                <BaseMultiselectWithValidation\r\n                    class=\"required\"\r\n                    v-model=\"activeDep.regionId\"\r\n                    :options=\"regions.map(e => e.id)\"\r\n                    @input=\"regionSelected\"\r\n                    :custom-label=\"customLabelRegion\"\r\n                    :placeholder=\"$t('column.region')\"\r\n                    open-direction=\"bottom\"\r\n                    :max-height=\"600\"\r\n                    :show-labels=\"false\"\r\n                    :only-form-element=\"true\"\r\n                />\r\n              </b-col>\r\n              <b-col cols=\"4\">\r\n                <!-- DISTRICTS -->\r\n                <BaseMultiselectWithValidation\r\n                    not-required\r\n                    v-model=\"activeDep.districtId\"\r\n                    :options=\"districts.map(e => e.id)\"\r\n                    @input=\"districtSelected\"\r\n                    :custom-label=\"customLabelDistrict\"\r\n                    :placeholder=\"$t('column.district')\"\r\n                    open-direction=\"bottom\"\r\n                    :max-height=\"600\"\r\n                    :show-labels=\"false\"\r\n                    :only-form-element=\"true\"\r\n                />\r\n              </b-col>\r\n\r\n            </b-row>\r\n\r\n          <!-- Table data -->\r\n          <b-table\r\n              :items=\"tableItems\"\r\n              :fields=\"tableFields\"\r\n              :busy=\"loadingTableItems\"\r\n              id=\"my-table\"\r\n              class=\"custom-b-table\"\r\n              responsive\r\n              striped\r\n              bordered\r\n              small\r\n              hover\r\n              show-empty\r\n          >\r\n            <!-- NUMBER OF ITEM -->\r\n            <template #cell(index)=\"data\">\r\n              {{ util_paginate(data.index, var_default_search_payload.page, var_default_search_payload.itemsPerPage) }}\r\n            </template>\r\n\r\n            <!-- NAME -->\r\n            <template #cell(name)=\"data\">\r\n              <div class=\"d-flex justify-content-between\">\r\n                <p\r\n                    class=\"mb-0 d-flex align-items-center\"\r\n                    style=\"flex-basis: 0; flex-grow: 1; gap: .3rem;\"\r\n                ><span class=\"badge bg-primary\">ЎЗ</span> : <span> {{ data.item.nameUz }}</span></p>\r\n                <p\r\n                    class=\"mb-0 d-flex align-items-center\"\r\n                    style=\"flex-basis: 0; flex-grow: 1; gap: .3rem;\"\r\n                ><span class=\"badge bg-primary\">O'Z</span> &nbsp;:&nbsp; <span> {{ data.item.nameLt }}</span></p>\r\n                <p\r\n                    class=\"mb-0 d-flex align-items-center\"\r\n                    style=\"flex-basis: 0; flex-grow: 1; gap: .3rem;\"\r\n                ><span class=\"badge bg-primary\">РУ</span> &nbsp;:&nbsp; <span> {{ data.item.nameRu }}</span></p>\r\n              </div>\r\n            </template>\r\n\r\n            <!-- STATUS -->\r\n            <template #cell(status)=\"data\">\r\n              <div class=\"d-flex justify-content-between\">\r\n                <p>\r\n                  {{\r\n                    getName({\r\n                      nameRu: data.item.statusNameRu,\r\n                      nameLt: data.item.statusNameLt,\r\n                      nameUz: data.item.statusNameUz,\r\n                    })\r\n                  }}\r\n                </p>\r\n              </div>\r\n            </template>\r\n\r\n            <!-- ADVERTISEMENT LOCATION TYPE NAME -->\r\n            <template #cell(advertisementLocationTypeName)=\"data\">\r\n              <div class=\"d-flex justify-content-between\">\r\n                <p>\r\n                  {{\r\n                    getName({\r\n                      nameRu: data.item.directoryAdvertisementLocationTypeNameRu,\r\n                      nameLt: data.item.directoryAdvertisementLocationTypeNameLt,\r\n                      nameUz: data.item.directoryAdvertisementLocationTypeNameUz,\r\n                    })\r\n                  }}\r\n                </p>\r\n              </div>\r\n            </template>\r\n\r\n            <!-- ADVERTISEMENT DESIGN TYPE NAME -->\r\n            <template #cell(advertisementDesignTypeName)=\"data\">\r\n              <div class=\"d-flex justify-content-between\">\r\n                <p>\r\n                  {{\r\n                    getName({\r\n                      nameRu: data.item.directoryAdvertisementDesignTypeNameRu,\r\n                      nameLt: data.item.directoryAdvertisementDesignTypeNameLt,\r\n                      nameUz: data.item.directoryAdvertisementDesignTypeNameUz,\r\n                    })\r\n                  }}\r\n                </p>\r\n              </div>\r\n            </template>\r\n\r\n            <!-- ADVERTISEMENT VOLUME TYPE NAME -->\r\n            <template #cell(advertisementVolumeTypeName)=\"data\">\r\n                <p>\r\n                    {{\r\n                getName({\r\n                  nameRu: data.item.directoryAdvertisementVolumeTypeNameRu,\r\n                  nameLt: data.item.directoryAdvertisementVolumeTypeNameLt,\r\n                  nameUz: data.item.directoryAdvertisementVolumeTypeNameUz,\r\n                })\r\n              }}\r\n                </p>\r\n            </template>\r\n\r\n            <!-- DISTRICT NAME -->\r\n            <template #cell(districtName)=\"data\">\r\n              <p>\r\n                {{\r\n                  getName({\r\n                    nameRu: data.item.districtNameRu,\r\n                    nameLt: data.item.districtNameLt,\r\n                    nameUz: data.item.districtNameUz,\r\n                  })\r\n                }}\r\n              </p>\r\n            </template>\r\n\r\n            <template #cell(costs)=\"data\">\r\n              <p>\r\n                {{ getNumberFormat(data.item.cost) }}\r\n              </p>\r\n            </template>\r\n\r\n            <!-- ACTIONS -->\r\n            <template #cell(actions)=\"data\">\r\n              <div class=\"general-table__actions d-flex justify-content-center\">\r\n                <b-btn\r\n                    variant=\"link\"\r\n                    class=\"text-decoration-none p-0\"\r\n                    style=\"font-size: 1.2rem; margin-right: 1rem;\"\r\n                >\r\n                  <i\r\n                      @click=\"editItem(data.item.id)\"\r\n                      class=\"mdi mdi-circle-edit-outline edit\"\r\n                  ></i>\r\n                </b-btn>\r\n\r\n                <b-btn\r\n                    variant=\"link\"\r\n                    class=\"text-decoration-none p-0 text-danger\"\r\n                    style=\"font-size: 1.2rem;\"\r\n                >\r\n                  <i\r\n                      @click=\"deleteItem(data.item.id)\"\r\n                      class=\"mdi mdi-trash-can delete\"\r\n                  ></i>\r\n                </b-btn>\r\n              </div>\r\n            </template>\r\n\r\n            <!-- EMPTY SLOT -->\r\n            <template #empty=\"\">\r\n              <h4 class=\"text-center\">{{ activeDep.regionId ? 'Маълумот топилмади' : 'Илтимос ҳудудни танланг' }}</h4>\r\n            </template>\r\n\r\n            <!-- TABLE_BUSY SLOT -->\r\n            <template #table-busy>\r\n              <div class=\"text-center my-2\">\r\n                <b-spinner variant=\"primary\" class=\"align-middle\"></b-spinner>\r\n                \r\n              </div>\r\n            </template>\r\n          </b-table>\r\n\r\n          <!-- end table -->\r\n          <b-pagination\r\n              v-model=\"var_default_search_payload.page\"\r\n              :total-rows=\"totalItems\"\r\n              :per-page=\"var_default_search_payload.itemsPerPage\"\r\n              aria-controls=\"my-table\"\r\n              class=\"justify-content-end\"\r\n          ></b-pagination>\r\n        </div>\r\n        <!-- end card-body -->\r\n      </div>\r\n      <!-- end card -->\r\n    </div>\r\n    <!-- end col -->\r\n  </div>\r\n  <!-- end row -->\r\n</template>\r\n\r\n<script>\r\nimport i18n from \"../../../../../../i18n\";\r\n\r\nconst MAIN_API_URL = 'directory/minimum-collections'\r\nimport appConfig from \"@/app.config\";\r\nimport crudAndListsService from '@/shared/services/crud_and_list.service'\r\nimport helperService from \"@/shared/services/helper.service\";\r\n\r\nexport default {\r\n  page: {\r\n    title: \"Zone Coefficients\",\r\n    meta: [{name: \"description\", content: appConfig.description}],\r\n  },\r\n  components: {},\r\n  data() {\r\n    return {\r\n      loadingTableItems: false,\r\n      json_fields: {\r\n        \"Қарор санаси\": \"decisionDate\",\r\n        \"Қарор рақами\": \"decisionNumber\",\r\n        \"Ташки реклама объекти (конструкцияси) жойлашуви\": \"directoryAdvertisementLocationTypeNameUz\",\r\n        \"Реклама конструкцияси номи\": \"directoryAdvertisementDesignTypeNameUz\",\r\n        \"Вилоят\": \"regionName\",\r\n        \"Туман\": \"districtName\",\r\n        \"Ўлчов бирлиги\": \"unitNameUz\",\r\n        \"Минимал йиғим (М)\": \"cost\",\r\n        \"Киритилган сана\": \"createdDate\",\r\n        \"Асос\": \"description\",\r\n      },\r\n      json_data: [],\r\n      json_meta: [\r\n        [\r\n          {\r\n            key: \"charset\",\r\n            value: \"utf-8\",\r\n          },\r\n        ],\r\n      ],\r\n      searchKeyword: '',\r\n      selected: 20,\r\n      optionsTable: [\r\n        {value: 20, text: 20},\r\n        {value: 50, text: 50},\r\n        {value: 100, text: 100},\r\n        {value: 150, text: 150},\r\n        {value: 200, text: 200},\r\n        {value: 'all', text: i18n.t('column.all')},\r\n      ],\r\n      title: \"Zone Coefficients\",\r\n      activeDep: {\r\n        regionId: '',\r\n        districtId: '',\r\n      },\r\n      regions: [],\r\n      districts: [],\r\n      tableItems: [],\r\n      totalItems: 0,\r\n      tableFields: [\r\n        {\r\n          label: \"#\",\r\n          thClass: \"text-center\",\r\n          tdClass: \"text-center\",\r\n          sortable: false,\r\n          key: \"index\",\r\n        },\r\n        {label: this.$t('column.decision_date'), key: \"decisionDate\"},\r\n        {label: this.$t('column.decision_number'), key: \"decisionNumber\"},\r\n        {label: this.$t('column.ad_location_type'), key: \"advertisementLocationTypeName\"},\r\n        {label: this.$t('column.ad_design_type'), key: \"advertisementDesignTypeName\"},\r\n        { label: this.$t('submodules.ad_volume_types.title'), key: \"advertisementVolumeTypeName\" },\r\n        {label: this.$t('column.district'), key: \"districtName\"},\r\n        // { label:this.$t('submodules.minimum_collection_types.title'), key: \"directoryMinimumCollectionTypeNameUz\" },\r\n        {label: this.$t('column.unit'), key: \"unitShortNameUz\"},\r\n        {label: this.$t('column.minimum_collection'), key: \"costs\"},\r\n        {label: this.$t('column.created_date'), key: \"createdDate\"},\r\n        {label: this.$t('column.status'), key: \"status\"},\r\n        {label: this.$t('column.reason'), key: \"description\"},\r\n        {\r\n          label: this.$t('column.actions'),\r\n          key: \"actions\",\r\n          thClass: \"text-center\",\r\n          tdClass: \"text-center\",\r\n          sortable: false\r\n        },\r\n      ],\r\n    };\r\n  },\r\n  /*\r\n  COMPUTED */\r\n  computed: {\r\n    numberOfPages() {\r\n      return (!this.totalItems || this.totalItems <= 0 || this.totalItems < this.var_default_search_payload.itemsPerPage) ? 1 : this.totalItems / this.var_default_search_payload.itemsPerPage\r\n    }\r\n  },\r\n  methods: {\r\n    downloadExcel() {\r\n      this.json_data = []\r\n      this.tableItems.forEach(res => {\r\n        let obj = {}\r\n        obj.decisionDate = res.decisionDate\r\n        obj.decisionNumber = res.decisionNumber\r\n        obj.directoryAdvertisementLocationTypeNameUz = res.directoryAdvertisementLocationTypeNameUz\r\n        obj.directoryAdvertisementDesignTypeNameUz = res.directoryAdvertisementDesignTypeNameUz\r\n        obj.regionName = res.regionName\r\n        obj.districtName = res.districtName\r\n        obj.unitNameUz = res.unitNameUz\r\n        obj.cost = res.cost\r\n        obj.createdDate = res.createdDate\r\n        obj.description = res.description\r\n        this.json_data.push(obj)\r\n      })\r\n    },\r\n    selectList($event) {\r\n      if ($event == 'all') {\r\n        this.var_default_search_payload.itemsPerPage = this.totalItems\r\n      } else {\r\n        this.var_default_search_payload.itemsPerPage = $event\r\n      }\r\n      this.fetchTableItems();\r\n    },\r\n    fetchTableItems() {\r\n      this.loadingTableItems = true\r\n      this.var_default_search_payload.keyword = this.searchKeyword\r\n      this.var_default_search_payload.regionId = this.activeDep.regionId\r\n      this.var_default_search_payload.districtId = this.activeDep.districtId\r\n      crudAndListsService\r\n          .searchListWithKeywordByRegionAndDistrict(MAIN_API_URL, this.var_default_search_payload)\r\n          .then((res) => {\r\n            this.tableItems = res.data.list;\r\n            this.totalItems = res.data.total;\r\n          })\r\n          .catch(e => {\r\n            this.tableItems = [];\r\n            this.totalItems = 0;\r\n          })\r\n          .finally(() => {\r\n            this.loadingTableItems = false\r\n          })\r\n    },\r\n    async regionSelected($event, dontResetDistrict = false) {\r\n      if (!dontResetDistrict) {\r\n        this.activeDep.districtId = null\r\n      }\r\n      // GET DISTRICTS\r\n      if ($event)\r\n        await helperService.getGeoLocationsByParentId($event)\r\n            .then(res => {\r\n              this.districts = res.data\r\n            })\r\n            .catch(e => {\r\n              console.log(e)\r\n            })\r\n      this.fetchTableItems()\r\n    },\r\n    async districtSelected() {\r\n      this.fetchTableItems()\r\n    },\r\n    customLabelRegion(opt) {\r\n      let selected = this.regions.find(e => e.id == opt);\r\n      if (selected) {\r\n        return `${\r\n            this.getName({\r\n              nameRu: selected.nameRu,\r\n              nameLt: selected.nameLt,\r\n              nameUz: selected.nameUz,\r\n            })\r\n        }`\r\n      }\r\n      return ``;\r\n    },\r\n    customLabelDistrict(opt) {\r\n      let selected = this.districts.find(e => e.id == opt);\r\n      if (selected) {\r\n        return `${\r\n            this.getName({\r\n              nameRu: selected.nameRu,\r\n              nameLt: selected.nameLt,\r\n              nameUz: selected.nameUz,\r\n            })\r\n        }`\r\n      }\r\n      return ``;\r\n    },\r\n    editItem(id) {\r\n      this.$router.push({name: 'UpdateMinimumCollection', params: {id: id}})\r\n    },\r\n    deleteItem(id) {\r\n      this.$bvModal.msgBoxConfirm(this.$t('messages.delete_title'), {\r\n        okTitle: this.$t('actions.confirm'),\r\n        cancelTitle: this.$t('actions.cancel')\r\n      })\r\n          .then(value => {\r\n            if (value) {\r\n              crudAndListsService\r\n                  .deleteById(MAIN_API_URL, id)\r\n                  .then((res) => {\r\n                    this.fetchTableItems()\r\n                  })\r\n                  .catch(e => {\r\n                    console.log(e)\r\n                  })\r\n            }\r\n          })\r\n          .catch(err => {\r\n            // An error occurred\r\n          })\r\n    },\r\n  },\r\n  /* CREATED */\r\n  async created() {\r\n    // GET REGIONS\r\n    await helperService.fetchRegions()\r\n        .then(res => {\r\n          this.regions = res.data\r\n        })\r\n        .catch(e => {\r\n          console.log(e)\r\n        })\r\n  },\r\n  /*\r\n  WATCH */\r\n  watch: {\r\n    'var_default_search_payload.page': {\r\n      handler() {\r\n        this.fetchTableItems()\r\n      }\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped lang='scss'>\r\n</style>"],"sourceRoot":"src/modules/references/modules/advertisement/modules/minimum_collections"}]}