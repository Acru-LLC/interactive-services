   loader: false\r\n    }\r\n  },\r\n  props: {\r\n    hasSaveFinish: {\r\n      type: Boolean,\r\n      default: false\r\n    },\r\n    hasSaveSuspend: {\r\n      type: Boolean,\r\n      default: false\r\n    },\r\n    hasSaveFinishBottom: {\r\n      type: Boolean,\r\n      default: false\r\n    },\r\n    hasSaveSuspendBottom: {\r\n      type: Boolean,\r\n      default: false\r\n    },\r\n    hasBackBottom: {\r\n      type: Boolean,\r\n      default: false\r\n    },\r\n    forParentInventoryDoc: {\r\n      type: Boolean,\r\n      default: false\r\n    },\r\n    customTitle: {\r\n      type: String,\r\n      default: ''\r\n    },\r\n    goBackRoute: {\r\n      type: Object,\r\n      default: () => {\r\n      }\r\n    }\r\n  },\r\n  /*\r\n  METHODS */\r\n  methods: {\r\n    emitSave() {\r\n      this.loader = true\r\n      bus.leaveWithConfirm = false\r\n      this.$emit('save')\r\n      setTimeout(() => {\r\n        bus.leaveWithConfirm = true\r\n        this.loader = false\r\n      }, 3000)\r\n    },\r\n    goBack() {\r\n      bus.leaveWithConfirm = true\r\n      if (this.goBackRoute && this.goBackRoute.name) {\r\n        this.$router.push(this.goBackRoute)\r\n      } else {\r\n        this.$router.go(-1)\r\n      }\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n</style>\r\n"]}]}