ame(proj)\">-->\r\n                                        <!--                                            {{ getName(proj) }}-->\r\n                                        <!--                                        </h5>-->\r\n                                        <span\r\n                                                class=\"badge\"\r\n                                                :class=\"'badge-primary'\"\r\n                                        >{{ $t(proj.status) }}</span>\r\n                                    </div>\r\n                                </div>\r\n                                <b-row>\r\n                                    <b-col cols=\"6\">\r\n                                        {{ $t('submodules.commission.inner_input_reg_number') }}\r\n                                        <p class=\"text-muted\">{{\r\n                                            proj.mnumber\r\n                                            }}</p>\r\n                                    </b-col>\r\n                                    <!--                                    <b-col cols=\"12\">-->\r\n                                    <!--                                        {{ $t('column.business_entity') }}-->\r\n                                    <!--                                        <p class=\"text-muted\">{{ proj.medicationName }}</p>-->\r\n                                    <!--                                    </b-col>-->\r\n                                </b-row>\r\n                                <!--                                <simplebar style=\"height: 100px\">-->\r\n                                <!--                                    <p class=\"text-muted\">{{ proj.description }}</p>-->\r\n                                <!--                                </simplebar>-->\r\n                                <!--                                <router-link-->\r\n                                <!--                                    