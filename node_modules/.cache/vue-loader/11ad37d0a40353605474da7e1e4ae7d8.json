is.form.dateTypeId = v;\r\n    },\r\n    checkValidity() {\r\n      this.submitted = true;\r\n      this.$v.form.$touch();\r\n      return this.$v.form.$anyError;\r\n    },\r\n    setFormData(item) {\r\n      this.form = {...this.form, ...item};\r\n      this.form.tableId = {\r\n        id: item.docTableId,\r\n        nameLt: item.docTableNameLt,\r\n        nameRu: item.docTableNameRu,\r\n        nameUz: item.docTableNameUz,\r\n      }\r\n      this.form.dateTypeId = {\r\n        id: item.dateTypeId,\r\n        nameLt: item.dateTypeNameLt,\r\n        nameRu: item.dateTypeNameRu,\r\n        nameUz: item.dateTypeNameUz,\r\n      }\r\n\r\n      this.getListDateTypesChild(item.parentDateTypeId)\r\n    },\r\n    async getListTable() {\r\n      await Service.getOwnTable(this.params)\r\n          .then((rs) => {\r\n            if (rs.data) {\r\n              this.valueList = rs.data.list;\r\n            }\r\n          })\r\n          .catch((e) => {});\r\n    },\r\n  },\r\n  created() {\r\n    // this.getListTable();\r\n  },\r\n  computed: {\r\n\r\n    params() {\r\n      return {\r\n        params: {\r\n          limit: 20,\r\n          page: 0,\r\n        },\r\n        search: this.search,\r\n      };\r\n    },\r\n    params2() {\r\n      return {\r\n        params: {\r\n          limit: 20,\r\n          page: 0,\r\n        },\r\n        search: this.search2,\r\n      };\r\n    },\r\n  },\r\n  data() {\r\n    return {\r\n      title: null,\r\n      search2: \"\",\r\n      list: [],\r\n      g_label_Uz: g_label_Uz,\r\n      submitted: false,\r\n      valueList: [],\r\n      search: \"\",\r\n      form: {\r\n        date: \"\",\r\n        tableId: null,\r\n        dateTypeId: null,\r\n        nameUz: \"\",\r\n        nameLt: \"\",\r\n        nameRu: \"\"\r\n      },\r\n    };\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n</style>\r\n"]}]}