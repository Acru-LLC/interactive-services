CA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"Index.vue","sourceRoot":"src/modules/integration/modules/open-data/modules/product-price-quotation","sourcesContent":["<template>\r\n  <b-row>\r\n    <b-col cols=\"12\">\r\n      <b-col md=\"12\" class=\"text-center\">\r\n        <div class=\"h4 mb-4 d-inline-block\">\r\n          {{ $t('open_data.product_price_quotation.code') }} - {{ $t('open_data.product_price_quotation.title') }}\r\n        </div>\r\n      </b-col>\r\n      <b-card>\r\n        <b-card-body>\r\n          <b-row class=\"mb-2\">\r\n            <div class=\"d-inline-block\">\r\n              <div class=\"search-box me-4 mb-2 d-inline-block\">\r\n                <div class=\"position-relative\">\r\n                  <b-input\r\n                      v-model=\"searchKeyword\"\r\n                      type=\"text\"\r\n                      class=\"form-control\"\r\n                      @input=\"fetchTableItems\"\r\n                      :placeholder=\"$t('column.search')\"\r\n                  />\r\n                  <i class=\"bx bx-search-alt search-icon\"></i>\r\n                </div>\r\n              </div>\r\n              <span>{{ $t('column.select.text1') }}</span>\r\n              <b-col cols=\"2\" class=\"me-2 mx-2 d-inline-block\">\r\n                <div class=\"position-relative\">\r\n                  <b-form-select\r\n                      v-model=\"selected\"\r\n                      :options=\"optionsTable\"\r\n                      @change=\"selectList\"\r\n                      class=\"form-select\"\r\n                  ></b-form-select>\r\n                </div>\r\n              </b-col>\r\n              <span>{{ $t('column.select.text2') }}</span>\r\n            </div>\r\n            <div class=\"d-inline-block\">\r\n              <div class=\"text-sm-end float-right mx-1\">\r\n                <b-btn\r\n                    @click=\"openImportExcelModal\"\r\n                    type=\"button\"\r\n                    class=\"btn btn-rounded mb-2\"\r\n                >\r\n                  <i class=\"mdi mdi-import me-1\"></i> {{ $t('actions.excel') }}\r\n                </b-btn>\r\n              </div>\r\n              <div class=\"text-sm-end float-right mx-1\">\r\n                <b-btn\r\n                    @click=\"sendOpenData\"\r\n                    type=\"button\"\r\n                    class=\"btn btn-rounded btn-pink mb-2\"\r\n                >\r\n                  <i class=\"mdi mdi-send me-1\"></i> {{ $t('actions.send') }}\r\n                </b-btn>\r\n                <b-btn\r\n                    @click=\"promptOpenDataToken\"\r\n                    type=\"button\"\r\n                    class=\"btn btn-rounded btn-pink mb-2\"\r\n                >\r\n                  <i class=\"mdi mdi-send me-1\"></i> {{ $t('actions.change_token') }}\r\n                </b-btn>\r\n              </div>\r\n              <div class=\"text-sm-end float-right\">\r\n                <download-excel\r\n                    :data=\"json_data\"\r\n                    :fields=\"json_fields\"\r\n                    :header=\"$t('open_data.product_price_quotation.title')\"\r\n                    worksheet=\"My Worksheet\"\r\n                    :name=\"`${$t('open_data.product_price_quotation.title')}.xls`\"\r\n                >\r\n                  <b-btn\r\n                      @click=\"downloadExcel\"\r\n                      type=\"button\"\r\n                      class=\"btn btn-rounded bg-primary mb-2\"\r\n                  >\r\n                    <i class=\"mdi mdi-microsoft-excel me-1\"></i> {{ $t('actions.download') }}\r\n                  </b-btn>\r\n                </download-excel>\r\n              </div>\r\n              <b-btn\r\n                  type=\"button\"\r\n                  class=\"btn btn-success btn-rounded mb-2 me-2 float-right\"\r\n                  :to=\"{name: 'CreateOpenDataProductPriceQuotation'}\"\r\n              >\r\n                <i class=\"mdi mdi-plus me-1\"></i> {{ $t('actions.add') }}\r\n              </b-btn>\r\n            </div>\r\n          </b-row>\r\n          <b-table\r\n              :items=\"tableItems\"\r\n              :fields=\"tableFields\"\r\n              :busy=\"loadingTableItems\"\r\n              id=\"my-table\"\r\n              class=\"custom-b-table\"\r\n              responsive\r\n              striped\r\n              bordered\r\n              small\r\n              hover\r\n              show-empty\r\n          >\r\n            <!-- NUMBER OF ITEM -->\r\n            <template #cell(index)=\"data\">\r\n              {{ util_paginate(data.index, var_default_search_payload.page, var_default_search_payload.itemsPerPage) }}\r\n            </template>\r\n\r\n            <!-- STATUS -->\r\n            <template #cell(status)=\"data\">\r\n              <p>\r\n                {{\r\n                  getName({\r\n                    nameRu: data.item.statusNameRu,\r\n                    nameLt: data.item.statusNameLt,\r\n                    nameUz: data.item.statusNameUz,\r\n                  })\r\n                }}\r\n              </p>\r\n            </template>\r\n\r\n            <!-- ACTIONS -->\r\n            <template #cell(actions)=\"data\">\r\n              <div class=\"general-table__actions d-flex justify-content-center\">\r\n                <b-btn\r\n                    variant=\"link\"\r\n                    class=\"text-decoration-none p-0\"\r\n                    style=\"font-size: 1.2rem; margin-right: 1rem;\"\r\n                >\r\n                  <i @click=\"viewItem(data.item.id)\" class=\"mdi mdi-eye-outline\"/>\r\n                </b-btn>\r\n                <b-btn\r\n                    variant=\"link\"\r\n                    class=\"text-decoration-none p-0\"\r\n                    style=\"font-size: 1.2rem; margin-right: 1rem;\"\r\n                >\r\n                  <i @click=\"editItem(data.item.id)\" class=\"mdi mdi-circle-edit-outline\"/>\r\n                </b-btn>\r\n                <b-btn\r\n                    variant=\"link\"\r\n                    class=\"text-decoration-none p-0 text-danger\"\r\n                    style=\"font-size: 1.2rem;\"\r\n                >\r\n                  <i @click=\"deleteItem(data.item.id)\" class=\"mdi mdi-trash-can-outline\"/>\r\n                </b-btn>\r\n              </div>\r\n            </template>\r\n\r\n            <!-- CHANGE INDEX -->\r\n            <template #cell(change_index)=\"data\">\r\n              <div class=\"d-flex justify-content-center\">\r\n                <b-btn\r\n                    @click=\"handleChangeIndex(data.item.id)\"\r\n                    variant=\"link\"\r\n                    class=\"text-decoration-none p-0\"\r\n                    style=\"font-size: 1.2rem; margin-right: 1rem;\"\r\n                >\r\n                  <i class=\"mdi\" :class=\"changeIndex.firstItemId === data.item.id || changeIndex.secondItemId === data.item.id ? 'mdi-checkbox-intermediate' : 'mdi-checkbox-blank-outline'\"/>\r\n                </b-btn>\r\n              </div>\r\n            </template>\r\n\r\n            <!-- EMPTY SLOT -->\r\n            <template #empty=\"\">\r\n              <h4 class=\"text-center\">{{ $t('messages.data_not_found') }}</h4>\r\n            </template>\r\n\r\n            <!-- TABLE_BUSY SLOT -->\r\n            <template #table-busy>\r\n              <div class=\"text-center my-2\">\r\n                <b-spinner\r\n                    variant=\"primary\"\r\n                    class=\"align-middle\"\r\n                />\r\n              </div>\r\n            </template>\r\n          </b-table>\r\n          <b-pagination\r\n              v-model=\"var_default_search_payload.page\"\r\n              :total-rows=\"totalItems\"\r\n              :per-page=\"var_default_search_payload.itemsPerPage\"\r\n              aria-controls=\"my-table\"\r\n              class=\"justify-content-end\"\r\n          ></b-pagination>\r\n        </b-card-body>\r\n      </b-card>\r\n      <b-modal\r\n          v-model=\"importExcelModal.isShow\"\r\n          size=\"lg\"\r\n          hide-footer\r\n          :title=\"$t('actions.upload_file')\"\r\n          :no-close-on-backdrop=\"true\"\r\n      >\r\n        <b-overlay\r\n            :opacity=\"0.1\"\r\n            :show=\"importExcelModal.loader\"\r\n            rounded=\"sm\"\r\n        >\r\n          <div class=\"row\">\r\n            <div class=\"col-md-12\">\r\n              <label for=\"\">{{ $t('actions.download_template') }}</label>\r\n              <div class=\"form-group\">\r\n                <a :href=\"`/template/opendata/${$t('open_data.product_price_quotation.code')}.xlsx`\" class=\"btn btn-info\" target=\"_blank\" download>{{ $t('actions.download_template') }}</a>\r\n              </div>\r\n            </div>\r\n            <div class=\"col-md-12\">\r\n              <label for=\"\">{{ $t('actions.choose_file') }}</label>\r\n              <file-input\r\n                  ref=\"refImportExcel\"\r\n              />\r\n            </div>\r\n            <div class=\"col-md-12\">\r\n              <div class=\"form-group\">\r\n                <b-btn variant=\"primary\" @click=\"onImportExcel\">{{ $t('actions.upload') }}</b-btn>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </b-overlay>\r\n      </b-modal>\r\n    </b-col>\r\n  </b-row>\r\n</template>\r\n\r\n<script>\r\nconst MAIN_API_URL = 'open-data/product-price-quotation'\r\nimport openDataService from '@/shared/services/open_data.service.js'\r\nimport crudAndListsService from '@/shared/services/crud_and_list.service'\r\nimport FileInput from '../../components/FileInput'\r\n\r\nexport default {\r\n  components: {\r\n    FileInput,\r\n  },\r\n  data() {\r\n    return {\r\n      loadingTableItems: false,\r\n      json_fields: {\r\n        [this.$t('open_data.product_price_quotation.productName')]: 'productName',\r\n        [this.$t('open_data.product_price_quotation.quotation')]: 'quotation',\r\n        [this.$t('open_data.product_price_quotation.dynamics')]: 'dynamics',\r\n        [this.$t('open_data.product_price_quotation.sum')]: 'sum',\r\n        [this.$t('open_data.product_price_quotation.percent')]: 'percent',\r\n        [this.$t('open_data.product_price_quotation.day')]: 'day',\r\n      },\r\n      json_data: [],\r\n      json_meta: [\r\n        [\r\n          {\r\n            key: \"charset\",\r\n            value: \"utf-8\",\r\n          },\r\n        ],\r\n      ],\r\n      searchKeyword: '',\r\n      selected: 20,\r\n      optionsTable: [\r\n        {value: 20, text: 20},\r\n        {value: 50, text: 50},\r\n        {value: 100, text: 100},\r\n        {value: 150, text: 150},\r\n        {value: 200, text: 200},\r\n      ],\r\n      activeDep: {},\r\n      changeIndex: {\r\n        firstItemId: null,\r\n        secondItemId: null,\r\n      },\r\n      tableItems: [],\r\n      totalItems: 0,\r\n      tableFields: [\r\n        {\r\n          label: \"#\",\r\n          thClass: \"text-center\",\r\n          tdClass: \"text-center\",\r\n          sortable: false,\r\n          key: \"index\",\r\n        },\r\n        {label: this.$t('open_data.product_price_quotation.productName'), key: 'productName'},\r\n        {label: this.$t('open_data.product_price_quotation.quotation'), key: 'quotation'},\r\n        {label: this.$t('open_data.product_price_quotation.dynamics'), key: 'dynamics'},\r\n        {label: this.$t('open_data.product_price_quotation.sum'), key: 'sum'},\r\n        {label: this.$t('open_data.product_price_quotation.percent'), key: 'percent'},\r\n        {label: this.$t('open_data.product_price_quotation.day'), key: 'day'},\r\n        {\r\n          label: this.$t('column.actions'),\r\n          key: \"actions\",\r\n          thClass: \"text-center\",\r\n          tdClass: \"text-center\",\r\n          sortable: false\r\n        },\r\n        {\r\n          label: '#',\r\n          key: \"change_index\",\r\n          thClass: \"text-center\",\r\n          tdClass: \"text-center\",\r\n        },\r\n      ],\r\n      importExcelModal: {\r\n        isShow: false,\r\n        loader: false,\r\n      },\r\n    };\r\n  },\r\n  computed: {\r\n    numberOfPages() {\r\n      return (!this.totalItems || this.totalItems <= 0 || this.totalItems < this.var_default_search_payload.itemsPerPage) ? 1 : this.totalItems / this.var_default_search_payload.itemsPerPage\r\n    }\r\n  },\r\n  methods: {\r\n    downloadExcel() {\r\n      this.json_data = [...this.tableItems];\r\n    },\r\n    openImportExcelModal() {\r\n      this.importExcelModal.isShow = true;\r\n    },\r\n    onImportExcel() {\r\n      if (!this.$refs.refImportExcel.file) {\r\n        return this.$toast(this.$t('messages.file_not_chosen'), {type: 'error'});\r\n      }\r\n      const formData = new FormData();\r\n      formData.append('uploadExcel', this.$refs.refImportExcel.file);\r\n      this.importExcelModal.loader = true;\r\n      crudAndListsService.post(MAIN_API_URL + \"/uploadByExcel\", formData, true)\r\n          .then(res => {\r\n            this.importExcelModal.loader = false;\r\n            this.importExcelModal.isShow = false;\r\n            return this.$toast(this.$t('messages.saved_successfully'), {type: 'success'});\r\n          })\r\n          .catch(err => {\r\n            this.importExcelModal.loader = false;\r\n            this.importExcelModal.isShow = false;\r\n            return this.$toast(this.$t('error.title'), {type: 'error'});\r\n          })\r\n          .finally(() => {\r\n            this.fetchTableItems()\r\n          });\r\n    },\r\n    sendOpenData() {\r\n      this.cnf().then(({value}) => {\r\n        if (value) {\r\n          this.fetchSendOpenData()\r\n        }\r\n      })\r\n    },\r\n    promptOpenDataToken(){\r\n      this.promptSwal({\r\n        title: this.$t('messages.enter_token'),\r\n        label: \" \",\r\n        buttonText: this.$t('actions.send')\r\n      }).then(({isConfirmed, value}) => {\r\n        if (isConfirmed) {\r\n          openDataService.createToken(this.$t('open_data.product_price_quotation.code'), value)\r\n        }\r\n      })\r\n    },\r\n    fetchSendOpenData() {\r\n      crudAndListsService\r\n          .post(MAIN_API_URL + \"/send-to-open-data\",{}, true)\r\n          .then(res => {\r\n            this.$toast(res.data);\r\n          })\r\n    },\r\n    async handleChangeIndex(id) {\r\n      if (this.changeIndex.firstItemId === null) {\r\n        this.changeIndex.firstItemId = id;\r\n      } else if (this.changeIndex.secondItemId === null && this.changeIndex.firstItemId !== id) {\r\n        this.changeIndex.secondItemId = id;\r\n      }\r\n      if (this.changeIndex.firstItemId !== null && this.changeIndex.secondItemId !== null && this.changeIndex.firstItemId !== this.changeIndex.secondItemId) {\r\n        await this.cnf().then(async ({isConfirmed}) => {\r\n          if (isConfirmed) {\r\n            await crudAndListsService.post(MAIN_API_URL + `/change-index?firstItemId=${this.changeIndex.firstItemId}&secondItemId=${this.changeIndex.secondItemId}`, null, true).then(async res => {\r\n              await this.fetchTableItems();\r\n            })\r\n          }\r\n          this.changeIndex.firstItemId = null;\r\n          this.changeIndex.secondItemId = null;\r\n        })\r\n      }\r\n    },\r\n    selectList($event) {\r\n      if ($event === 'all') {\r\n        this.var_default_search_payload.itemsPerPage = this.totalItems\r\n      } else {\r\n        this.var_default_search_payload.itemsPerPage = $event\r\n      }\r\n      this.fetchTableItems();\r\n    },\r\n    fetchTableItems() {\r\n      this.loadingTableItems = true\r\n      this.var_default_search_payload.keyword = this.searchKeyword\r\n      this.var_default_search_payload.sortDesc.push(false)\r\n      this.var_default_search_payload.sortBy.push('productName')\r\n      crudAndListsService\r\n          .searchListWithKeyword(MAIN_API_URL, this.var_default_search_payload)\r\n          .then((res) => {\r\n            this.tableItems = res.data.list;\r\n            this.totalItems = res.data.total;\r\n          })\r\n          .catch(e => {\r\n            this.tableItems = [];\r\n            this.totalItems = 0;\r\n          })\r\n          .finally(() => {\r\n            this.loadingTableItems = false\r\n          })\r\n    },\r\n    editItem(id) {\r\n      this.$router.push({name: 'UpdateOpenDataProductPriceQuotation', params: {id: id}})\r\n    },\r\n    viewItem(id) {\r\n      this.$router.push({name: 'ViewOpenDataProductPriceQuotation', params: {id: id}})\r\n    },\r\n    deleteItem(id) {\r\n      this.$bvModal.msgBoxConfirm(this.$t('messages.delete_title'), {\r\n        okTitle: this.$t('actions.confirm'),\r\n        cancelTitle: this.$t('actions.cancel')\r\n      })\r\n          .then(value => {\r\n            if (value) {\r\n              crudAndListsService\r\n                  .deleteById(MAIN_API_URL, id)\r\n                  .then((res) => {\r\n                    this.fetchTableItems()\r\n                  })\r\n                  .catch(e => {\r\n                    console.log(e)\r\n                  })\r\n            }\r\n          })\r\n          .catch(err => {\r\n            // An error occurred\r\n          })\r\n    },\r\n  },\r\n  created() {\r\n    this.fetchTableItems()\r\n  },\r\n  watch: {\r\n    'var_default_search_payload.page': {\r\n      handler() {\r\n        this.fetchTableItems()\r\n      },\r\n      deep: true\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped lang='scss'>\r\n</style>"]}]}