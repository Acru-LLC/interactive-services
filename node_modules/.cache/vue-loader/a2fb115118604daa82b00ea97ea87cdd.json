mnAndRows>\r\n                </div>\r\n              </div>\r\n\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n\r\n      <div class=\"row\">\r\n        <div class=\"col-12\">\r\n          <div class=\"card\">\r\n            <div class=\"card-body\">\r\n              <div class=\"row\">\r\n                <div class=\"col-lg-4\">\r\n                  <!--                                    <h5>{{ $t(\"template_departments\") }}</h5>-->\r\n                  <!-- <Back :to=\"'/report/templates'\" /> -->\r\n                </div>\r\n                <div class=\"col-lg-8\">\r\n                  <div class=\"text-sm-right\">\r\n\r\n                  </div>\r\n                </div>\r\n              </div>\r\n              <div class=\"row\">\r\n                <div class=\"col-12\">\r\n                  <ol class=\"mt-2 pl-3\">\r\n                    <li\r\n                        class=\"p-2 lidep\"\r\n                        v-for=\"(item, index) in listDepartments\"\r\n                        :key=\"index + 'Departments'\"\r\n                    >\r\n                      {{\r\n                        getName({\r\n                          nameRu: item.nameRu,\r\n                          nameLt: item.nameLt,\r\n                          nameUz: item.nameUz,\r\n                        })\r\n                      }}\r\n                      <!-- <b-button\r\n  @click=\"openConf\"\r\n  class=\"li-hide ml-2\"\r\n  size=\"sm\"\r\n  variant=\"light\"\r\n>\r\n  <i class=\"bx bx-trash\"></i>\r\n</b-button> -->\r\n                    </li>\r\n                  </ol>\r\n                  <Back :to=\"'/report/templates'\"/>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<style>\r\n.lidep:hover .li-hide {\r\n  visibility: visible;\r\n}\r\n\r\n.lidep:hover {\r\n  font-weight: bold;\r\n  cursor: pointer;\r\n}\r\n\r\n.li-hide